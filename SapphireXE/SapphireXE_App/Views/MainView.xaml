                        </TabItem>
                        <!-- Report 화면 -->
                        <TabItem Header="Report" Width="120">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="300"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Grid Grid.Column="0" Background="{DynamicResource PrimaryBackgroundColor}" HorizontalAlignment="Right" Width="300">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="240"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <!-- Plot 콘트롤 버턴 배치-->
                                    <StackPanel Grid.Row="0">
                                        <StackPanel.Resources>
                                            <Style TargetType="{x:Type Button}">
                                                <Setter Property="Background" Value="#FF0078D7"/>
                                                <Setter Property="Foreground" Value="White"/>
                                                <Setter Property="Width" Value="70"/>
                                                <Setter Property="Margin" Value="5,5,5,2"/>
                                                <Style.Resources>
                                                    <Style TargetType="{x:Type Border}">
                                                        <Setter Property="CornerRadius" Value="6"/>
                                                    </Style>
                                                </Style.Resources>
                                            </Style>
                                        </StackPanel.Resources>
                                        <StackPanel Height="30">
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <Button x:Name="PlotChart" Content="Plot" >
                                                </Button>
                                                <Button x:Name="PlotSave" Content="Save" >
                                                </Button>
                                            </StackPanel>
                                        </StackPanel>
                                        <StackPanel>
                                            <TextBlock FontSize="14" Margin="10,20,0,0" Foreground="Black" Text="Recipe File"/>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" >
                                                <TextBox x:Name="PlotRecipeFileOpen_1" Width="200" Height="25" VerticalAlignment="Bottom">
                                                    <TextBox.InputBindings>
                                                        <MouseBinding	Command="{Binding PlotRecipeFileOpenCommand}" MouseAction="LeftDoubleClick"/>
                                                    </TextBox.InputBindings>
                                                </TextBox>
                                                <Button x:Name="PlotRecipeFileClose_1" Content="Remove" VerticalAlignment="Center"
										Command="{Binding PlotRecipeFileRemoveCommand}">
                                                </Button>
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <TextBox x:Name="PlotRecipeFileOpen_2" Width="200" Height="25" VerticalAlignment="Bottom">
                                                    <TextBox.InputBindings>
                                                        <MouseBinding	Command="{Binding PlotRecipeFileOpenCommand}" MouseAction="LeftDoubleClick"/>
                                                    </TextBox.InputBindings>
                                                </TextBox>
                                                <Button x:Name="PlotRecipeFileClose_2" Content="Remove" VerticalAlignment="Center"
										Command="{Binding PlotRecipeFileRemoveCommand}">
                                                </Button>
                                            </StackPanel>
                                            <TextBlock FontSize="14" Margin="10,20,0,0" Foreground="Black" Text="Log File"/>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <TextBox x:Name="PlotLogFileOpen_1" Width="200" Height="25" VerticalAlignment="Bottom"
										 Text="{Binding Path=logFilepath1, UpdateSourceTrigger=PropertyChanged}">
                                                    <TextBox.InputBindings>
                                                        <MouseBinding	Command="{Binding PlotLogFileOpen1Command}" MouseAction="LeftDoubleClick"/>
                                                    </TextBox.InputBindings>
                                                </TextBox>
                                                <Button x:Name="PlotLogFileClose_1" Content="Remove" VerticalAlignment="Center"
										Command="{Binding PlotLogFileRemove1Command}">
                                                </Button>
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <TextBox x:Name="PlotLogFileOpen_2" Width="200" Height="25" VerticalAlignment="Bottom"
										 Text="{Binding Path=logFilepath2,  UpdateSourceTrigger=PropertyChanged}">
                                                    <TextBox.InputBindings>
                                                        <MouseBinding	Command="{Binding PlotLogFileOpen2Command}" MouseAction="LeftDoubleClick"/>
                                                    </TextBox.InputBindings>
                                                </TextBox>
                                                <Button x:Name="PlotLogFileClose_2" Content="Remove" VerticalAlignment="Center"
										Command="{Binding PlotLogFileRemove2Command}">
                                                </Button>
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                <Button Content="Data1" x:Name="btnPlotData1" VerticalContentAlignment="Center" Width="50"
										Command="{Binding btnPlotData1Command}"/>
                                                <Button Content="Data2" x:Name="btnPlotData2" VerticalContentAlignment="Center" Width="50"
										Command="{Binding btnPlotData2Command}"/>
                                                <Button Content="Data3" x:Name="btnPlotData3" VerticalContentAlignment="Center" Width="50"
										Command="{Binding btnPlotData3Command}"/>
                                                <Button Content="Data4" x:Name="btnPlotData4" VerticalContentAlignment="Center" Width="50"
										Command="{Binding btnPlotData4Command}"/>
                                            </StackPanel>

                                        </StackPanel>
                                    </StackPanel>
                                    <Grid Grid.Row="1">
                                        <StackPanel>
                                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                                <StackPanel.Resources>
                                                    <Style TargetType="{x:Type Button}">
                                                        <Setter Property="Background" Value="#FF0078D7"/>
                                                        <Setter Property="Foreground" Value="White"/>
                                                        <Setter Property="Width" Value="70"/>
                                                        <Setter Property="Height" Value="20"/>
                                                        <Setter Property="Margin" Value="5,5,5,2"/>
                                                        <Style.Resources>
                                                            <Style TargetType="{x:Type Border}">
                                                                <Setter Property="CornerRadius" Value="6"/>
                                                            </Style>
                                                        </Style.Resources>
                                                    </Style>
                                                </StackPanel.Resources>
                                                <Button x:Name="btnPlotDevice" Content="Plot Device" Width="70" Margin="5"
										Command="{Binding btnPlotDeviceCommand}"/>
                                                <TextBlock FontSize="14" Margin="10" Foreground="Black" Text="Time"/>
                                                <TextBox x:Name="PlotTime" Width="70" Height="25" />
                                            </StackPanel>
                                            <control:DeviceSelect/>

                                            <DataGrid x:Name="PlotSelectedDevice" AutoGenerateColumns="False" Padding="5"
										CanUserAddRows="False" AlternationCount="2" AlternatingRowBackground="LightGray" 
										ItemsSource="{Binding ReportCompareData}">
                                                <DataGrid.Resources>
                                                    <Style TargetType="{x:Type DataGridColumnHeader}">
                                                        <Setter Property="HorizontalAlignment" Value="Center"/>
                                                        <Setter Property="Background" Value="Transparent"/>
                                                    </Style>
                                                    <Style TargetType="{x:Type DataGrid}">
                                                        <Setter Property="BorderBrush" Value="Black"/>
                                                        <Setter Property="BorderThickness" Value="1"/>
                                                    </Style>
                                                </DataGrid.Resources>
                                                <DataGrid.Columns>
                                                    <DataGridTextColumn Width="*" Header="TimeStamp" Binding="{Binding TimeStamp, StringFormat=\{0:hh:mm:ss\}}"/>
                                                    <DataGridTextColumn Width="*" Header="Data1" Binding="{Binding Data1, StringFormat=\{0:0.0000\}}"/>
                                                    <DataGridTextColumn Width="*" Header="Data2" Binding="{Binding Data2, StringFormat=\{0:0.0000\}}"/>
                                                    <DataGridTextColumn Width="*" Header="Data3" Binding="{Binding Data3, StringFormat=\{0:0.0000\}}"/>
                                                    <DataGridTextColumn Width="*" Header="Data4" Binding="{Binding Data4, StringFormat=\{0:0.0000\}}"/>
                                                </DataGrid.Columns>
                                            </DataGrid>
                                        </StackPanel>
                                    </Grid>
                                </Grid>
                                <Grid Grid.Column="1" Background="{DynamicResource PrimaryBackgroundColor}">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="99*"/>
                                        <ColumnDefinition Width="907*"/>
                                    </Grid.ColumnDefinitions>
                                    <oxy:PlotView Model="{Binding ReportComparePlot}" Grid.ColumnSpan="2">
                                        <!--<b:Interaction.Triggers>
					<b:EventTrigger EventName="MouseLeftButtonDown">
						<b:InvokeCommandAction Command="{Binding ResetAxisCommand}"/>
					</b:EventTrigger>
				</b:Interaction.Triggers>-->
                                    </oxy:PlotView>

                                </Grid>
                            </Grid>
                        </TabItem>
                        <!-- Setting 화면 -->
                        <TabItem Header="Setting" Width="120">
                            <Grid>
                                <TabControl TabStripPlacement="Left">
                                    <TabControl.Resources>
                                        <Style TargetType="{x:Type TabItem}">
                                            <Setter Property="HeaderTemplate">
                                                <Setter.Value>
                                                    <DataTemplate>
                                                        <ContentPresenter Content="{TemplateBinding Content}">
                                                            <ContentPresenter.LayoutTransform>
                                                                <RotateTransform Angle="270"/>
                                                            </ContentPresenter.LayoutTransform>
                                                        </ContentPresenter>
                                                    </DataTemplate>
                                                </Setter.Value>
                                            </Setter>
                                            <Setter Property="Width" Value="20"/>
                                            <Setter Property="Height" Value="90"/>
                                            <Setter Property="Padding" Value="3"/>
                                        </Style>
                                    </TabControl.Resources>
                                    <!--Tab for Alarm Setting -->
                                    <TabItem Name="AlarmSetting" Header="Alarm Setting">
                                        <TabItem.Content>
                                            <Grid Width="1290">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="30"/>
                                                    <RowDefinition Height="*"/>
                                                </Grid.RowDefinitions>
                                                <Grid Grid.Row="0" Background="Orange">
                                                    <TextBlock FontSize="14" Text="Alarm / Warning Setting" FontWeight="Bold" Foreground="White"
                                           VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Grid>
                                                <Grid Grid.Row="1">
                                                    <StackPanel Orientation="Horizontal">
                                                        <!-- 알람(Analog) 설정  -->
                                                        <GroupBox Header="Alarm Setting (Analog)" Width="430"
                                          BorderBrush="LightSteelBlue" BorderThickness="2" Margin="2,10,2,0"
                                              FontSize="14">
                                                            <Grid>
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition Height="25"/>
                                                                    <RowDefinition Height="25"/>
                                                                    <RowDefinition/>
                                                                </Grid.RowDefinitions>
                                                                <StackPanel Grid.Row="0" Orientation="Horizontal">
                                                                    <TextBlock FontSize="12" Height="25" Width="120" Text="Delay Time(s)"/>
                                                                    <TextBox x:Name="tbAlarmDelayTimeA" Text="{Binding Path=dPreSet[AlarmDelayTimeA]}" Width="70" Height="20"/>
                                                                    <Button x:Name="btnAlarmSettingSave" Content="Save" Width="70" HorizontalAlignment="Right"
															Margin="10,0,0,0" Command="{Binding AlarmSettingSaveCommand}">
                                                                        <Button.Resources>
                                                                            <Style TargetType="Border">
                                                                                <Setter Property="CornerRadius" Value="6"/>
                                                                            </Style>
                                                                        </Button.Resources>
                                                                    </Button>
                                                                </StackPanel>
                                                                <StackPanel Grid.Row="1" Orientation="Horizontal">
                                                                    <TextBlock FontSize="12" Height="25" Width="120" Text="Alarm Deviation(%)" />
                                                                    <TextBox x:Name="tbAlarmDeviatin" Text="{Binding Path=dPreSet[AlarmDeviation]}" Width="70" Height="20"/>
                                                                    <TextBlock FontSize="12" Height="25" Width="120" Margin="10,0,5,0"
                                                           Text="Warning Deviation(%)" />
                                                                    <TextBox x:Name="tbWarningDeviation" Text="{Binding Path=dPreSet[WarningDeviation]}" Width="70" Height="20"/>
                                                                </StackPanel>
                                                                <StackPanel Grid.Row="2">
                                                                    <DataGrid x:Name="dgAlarmSettingA" AutoGenerateColumns="False" Margin="5,0,5,0"
																VerticalScrollBarVisibility="Auto" ItemsSource="{Binding lGasAIO}" CanUserAddRows="False">
                                                                        <DataGrid.Resources>
                                                                            <Style TargetType="{x:Type DataGridColumnHeader}">
                                                                                <Setter Property="HorizontalAlignment" Value="Center"/>
                                                                                <Setter Property="Background" Value="Transparent"/>
                                                                            </Style>
                                                                            <Style TargetType="{x:Type DataGrid}">
                                                                                <Setter Property="BorderBrush" Value="Black"/>
                                                                                <Setter Property="BorderThickness" Value="1"/>
                                                                            </Style>
                                                                        </DataGrid.Resources>
                                                                        <DataGrid.Columns>
                                                                            <DataGridTextColumn Header="ID" CanUserSort="False" CanUserResize="False" Width="50"
																							Binding="{Binding Path=ID}"/>
                                                                            <DataGridTextColumn Header="Device Name" CanUserSort="False" CanUserResize="False" Width="*"
																							Binding="{Binding Path=Name}"/>
                                                                            <DataGridCheckBoxColumn Header="Alarm" Width="50"	CanUserSort="False"
																									Binding="{Binding Path=AlarmSet}"/>
                                                                            <DataGridCheckBoxColumn Header="Warning" Width="50"	CanUserSort="False"
																									Binding="{Binding Path=WarningSet}"/>
                                                                        </DataGrid.Columns>
                                                                    </DataGrid>
                                                                </StackPanel>
                                                            </Grid>
                                                        </GroupBox>
                                                        <!-- 알람(Digital) 설정 -->
                                                        <GroupBox Header="Alarm Setting (Digital)" Width="430"
                                          BorderBrush="LightSteelBlue" BorderThickness="2" Margin="2,10,2,0"
                                          FontSize="14">
                                                            <Grid>
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition Height="25"/>
                                                                    <RowDefinition Height="25"/>
                                                                    <RowDefinition/>
                                                                </Grid.RowDefinitions>
                                                                <StackPanel Grid.Row="0" Orientation="Horizontal">
                                                                    <TextBlock FontSize="12" Height="25" Width="120" Text="Delay Time(s)"/>
                                                                    <TextBox x:Name="tbAlarmDelayTimeD" Text="{Binding Path=dPreSet[AlarmDelayTimeD]}" Width="70" Height="20"/>
                                                                    <Button x:Name="btnEventSettingSave" Content="Save" Width="70" HorizontalAlignment="Right"
															Margin="10,0,0,0" Command="{Binding  AlarmSettingSaveCommand}">
                                                                        <Button.Resources>
                                                                            <Style TargetType="Border">
                                                                                <Setter Property="CornerRadius" Value="6"/>
                                                                            </Style>
                                                                        </Button.Resources>
                                                                    </Button>
                                                                </StackPanel>
                                                                <StackPanel Grid.Row="2">
                                                                    <DataGrid x:Name="dgAlarmSettingD" AutoGenerateColumns="False" Margin="5,0,5,0"
                                                     VerticalScrollBarVisibility="Auto" ItemsSource="{Binding lSwitchDI}" CanUserAddRows="False">
                                                                        <DataGrid.Resources>
                                                                            <Style TargetType="{x:Type DataGridColumnHeader}">
                                                                                <Setter Property="HorizontalAlignment" Value="Center"/>
                                                                                <Setter Property="Background" Value="Transparent"/>
                                                                            </Style>
                                                                        </DataGrid.Resources>

                                                                        <DataGrid.Columns>
                                                                            <DataGridTextColumn Header="ID" CanUserSort="False" CanUserResize="False" Width="50"
                                                                        Binding="{Binding Path=ID}"/>
                                                                            <DataGridTextColumn Header="Device Name" CanUserSort="False" CanUserResize="False" Width="*"
                                                                        Binding="{Binding Path=Name}"/>
                                                                            <DataGridCheckBoxColumn Header="Alarm" CanUserSort="False" CanUserResize="False" Width="50"
                                                                        Binding="{Binding Path=AlarmSet}"/>
                                                                            <DataGridCheckBoxColumn Header="Warning" CanUserSort="False"  CanUserResize="False" Width="50"
                                                                        Binding="{Binding Path=WarningSet}"/>
                                                                        </DataGrid.Columns>
                                                                    </DataGrid>
                                                                </StackPanel>
                                                            </Grid>
                                                        </GroupBox>
                                                        <!-- LogBook 테이블 -->
                                                        <GroupBox Header="LogBook" Width="420"
                                          BorderBrush="LightSteelBlue" BorderThickness="2" Margin="2,10,2,0"
                                          FontSize="14">
                                                            <Grid>
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition Height="25"/>
                                                                    <RowDefinition Height="25"/>
                                                                    <RowDefinition/>
                                                                </Grid.RowDefinitions>
                                                                <StackPanel Grid.Row="0">
                                                                    <StackPanel.Resources>
                                                                        <Style TargetType="{x:Type Button}">
                                                                            <Style.Resources>
                                                                                <Style TargetType="{x:Type Border}">
                                                                                    <Setter Property="CornerRadius" Value="4"/>
                                                                                </Style>
                                                                            </Style.Resources>
                                                                        </Style>
                                                                    </StackPanel.Resources>
                                                                    <Button x:Name="LogFileRead" Content="Read" Width="70" Margin="25,0,0,0">
                                                                    </Button>
                                                                </StackPanel>

                                                                <StackPanel Grid.Row="1" Orientation="Horizontal" >
                                                                    <TextBlock FontSize="12" Height="25" Width="70" Text="Filter"/>
                                                                    <Button x:Name="LogFilterAlarm" Content="Alarm" Width="50" Margin="25,0,0,0" Background="red"/>
                                                                    <Button x:Name="LogFilterWarning" Content="Warning" Width="50" Margin="25,0,0,0" Background="Orange"/>
                                                                    <Button x:Name="LogFilterInformation" Content="Information" Width="50" Margin="25,0,0,0" Background="Green"/>
                                                                    <Button x:Name="LogFilterClear" Content="Clear" Width="50" Margin="25,0,0,0" Background="LightGray"/>
                                                                </StackPanel>
                                                                <StackPanel Grid.Row="2">
                                                                    <DataGrid x:Name="LogbookTable" AutoGenerateColumns="False" Margin="5,0,5,0">
                                                                        <DataGrid.Resources>
                                                                            <Style TargetType="{x:Type DataGridColumnHeader}">
                                                                                <Setter Property="HorizontalAlignment" Value="Center"/>
                                                                                <Setter Property="Background" Value="Transparent"/>
                                                                            </Style>
                                                                        </DataGrid.Resources>
                                                                        <DataGrid.Columns>
                                                                            <DataGridTextColumn Header="Type" CanUserSort="False" CanUserResize="False" Width="60"
                                                                        Binding="{Binding}"/>
                                                                            <DataGridTextColumn Header="Massage" CanUserSort="False" CanUserResize="False" Width="*"
                                                                        Binding="{Binding}"/>
                                                                            <DataGridTextColumn Header="Date" CanUserSort="False" CanUserResize="False" Width="100"
                                                                        Binding="{Binding Date, StringFormat=d}"/>
                                                                            <DataGridTextColumn Header="Time" CanUserSort="False" CanUserResize="False" Width="100"
                                                                        Binding="{Binding Time}"/>
                                                                        </DataGrid.Columns>
                                                                    </DataGrid>
                                                                </StackPanel>
                                                            </Grid>
                                                        </GroupBox>
                                                    </StackPanel>
                                                </Grid>
                                            </Grid>
                                        </TabItem.Content>

                                    </TabItem>
                                    <!--Tab for Safety Setting-->
                                    <TabItem Name="UserStateSetting" Header="Safety Setting">
                                        <TabItem.Content>
                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="30"/>
                                                    <RowDefinition/>
                                                </Grid.RowDefinitions>
                                                <Grid Grid.Row="0" Background="Green">
                                                    <TextBlock FontSize="14" Text="Safety Setting" FontWeight="Bold" Foreground="White"
                                           VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Grid>
                                                <Grid Grid.Row="1">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="28*"/>
                                                        <RowDefinition Height="723*"/>
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="21*"/>
                                                        <ColumnDefinition Width="13*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <StackPanel Orientation="Horizontal" Grid.ColumnSpan="2" Grid.RowSpan="2">
                                                        <!-- User State 설정 테이블 -->
                                                        <GroupBox Header="User State Setting" Width="640" 
                                          BorderBrush="LightSteelBlue" BorderThickness="2" Margin="2,10,2,0"
                                          FontSize="14">
                                                            <StackPanel Grid.Column="0">
                                                                <Button x:Name="UserStateSave" Content="Save" Width="70" Margin="5" HorizontalAlignment="Right"
                                                 Command="{Binding AlarmSettingSaveCommand}">
                                                                    <Button.Resources>
                                                                        <Style TargetType="Border">
                                                                            <Setter Property="CornerRadius" Value="6"/>
                                                                        </Style>
                                                                    </Button.Resources>
                                                                </Button>
                                                                <Grid Height="670">
                                                                    <Grid.RowDefinitions>
                                                                        <RowDefinition Height="50*"/>
                                                                        <RowDefinition Height="Auto"/>
                                                                        <RowDefinition Height="50*"/>
                                                                    </Grid.RowDefinitions>
                                                                    <StackPanel Grid.Row="0">
                                                                        <DataGrid x:Name="dgUserSettingA" AutoGenerateColumns="False" Margin="5,0,5,0" Height="320"
                                                          VerticalScrollBarVisibility="Auto" ItemsSource="{Binding lGasAIO}" CanUserAddRows="False">
                                                                            <DataGrid.Resources>
                                                                                <Style TargetType="{x:Type DataGridColumnHeader}">
                                                                                    <Setter Property="HorizontalAlignment" Value="Center"/>
                                                                                    <Setter Property="Background" Value="Transparent"/>
                                                                                </Style>
                                                                                <Style TargetType="{x:Type DataGrid}">
                                                                                    <Setter Property="BorderBrush" Value="Black"/>
                                                                                    <Setter Property="BorderThickness" Value="1"/>
                                                                                </Style>
                                                                            </DataGrid.Resources>
                                                                            <DataGrid.Columns>
                                                                                <DataGridTextColumn Header="Device Id" CanUserSort="False" CanUserResize="False" Width="60"
                                                                            Binding="{Binding Path=ID, Mode=TwoWay}"/>
                                                                                <DataGridTextColumn Header="Device Name" CanUserSort="False" CanUserResize="False" Width="*"
                                                                            Binding="{Binding Path=Name}"/>
                                                                                <DataGridTextColumn Header="State 1" CanUserSort="False" CanUserResize="False" Width="100"
                                                                            Binding="{Binding Path=UserState1}"/>
                                                                                <DataGridTextColumn Header="State 2" CanUserSort="False" CanUserResize="False" Width="100"
                                                                            Binding="{Binding Path=UserState2}"/>
                                                                                <DataGridTextColumn Header="State 3" CanUserSort="False" CanUserResize="False" Width="100"
                                                                            Binding="{Binding Path=UserState3}"/>
                                                                                <DataGridTextColumn Header="State 4" CanUserSort="False" CanUserResize="False" Width="100"
                                                                            Binding="{Binding Path=UserState4}"/>
                                                                            </DataGrid.Columns>
                                                                        </DataGrid>
                                                                    </StackPanel>
                                                                    <StackPanel Grid.Row="1" Height="15">
                                                                    </StackPanel>
                                                                    <StackPanel Grid.Row="2">
                                                                        <DataGrid x:Name="dgUserSettingD" AutoGenerateColumns="False" Margin="5,0,5,0" Height="320"
                                                          VerticalScrollBarVisibility="Auto" ItemsSource="{Binding lGasDO}" CanUserAddRows="False">
                                                                            <DataGrid.Resources>
                                                                                <Style TargetType="{x:Type DataGridColumnHeader}">
                                                                                    <Setter Property="HorizontalAlignment" Value="Center"/>
                                                                                    <Setter Property="Background" Value="Transparent"/>
                                                                                </Style>
                                                                                <Style TargetType="{x:Type DataGrid}">
                                                                                    <Setter Property="BorderBrush" Value="Black"/>
                                                                                    <Setter Property="BorderThickness" Value="1"/>
                                                                                </Style>
                                                                            </DataGrid.Resources>
                                                                            <DataGrid.Columns>
                                                                                <DataGridTextColumn Header="Device Id" CanUserSort="False" CanUserResize="False" Width="60"
                                                                            Binding="{Binding Path=ID, Mode=TwoWay}"/>
                                                                                <DataGridTextColumn Header="Device Name" CanUserSort="False" CanUserResize="False" Width="*"
                                                                            Binding="{Binding Path=Name}"/>
                                                                                <DataGridCheckBoxColumn Header="State 1" CanUserSort="False" CanUserResize="False" Width="100"
                                                                            Binding="{Binding Path=UserState1}"/>
                                                                                <DataGridCheckBoxColumn Header="State 2" CanUserSort="False" CanUserResize="False" Width="100"
                                                                            Binding="{Binding Path=UserState2}"/>
                                                                                <DataGridCheckBoxColumn Header="State 3" CanUserSort="False" CanUserResize="False" Width="100"
                                                                            Binding="{Binding Path=UserState3}"/>
                                                                                <DataGridCheckBoxColumn Header="State 4" CanUserSort="False" CanUserResize="False" Width="100"
                                                                            Binding="{Binding Path=UserState4}"/>
                                                                            </DataGrid.Columns>
                                                                        </DataGrid>
                                                                    </StackPanel>
                                                                </Grid>
                                                            </StackPanel>
                                                        </GroupBox>
                                                        <StackPanel>
                                                            <!-- Interlock 설정 테이블 -->
                                                            <GroupBox Header="Interlock Setting" Width="640" BorderBrush="LightSteelBlue" BorderThickness="2" Margin="2,10,2,0"
                                              FontSize="14">
                                                                <Grid>
                                                                    <Grid.ColumnDefinitions>
                                                                        <ColumnDefinition/>
                                                                        <ColumnDefinition/>
                                                                    </Grid.ColumnDefinitions>
                                                                    <Grid.RowDefinitions>
                                                                        <RowDefinition Height="35"/>
                                                                        <RowDefinition Height="30"/>
                                                                        <RowDefinition Height="30"/>
                                                                        <RowDefinition Height="30"/>
                                                                        <RowDefinition Height="30"/>
                                                                        <RowDefinition Height="30"/>
                                                                        <RowDefinition Height="30"/>
                                                                        <RowDefinition Height="30"/>
                                                                        <RowDefinition Height="30"/>
                                                                        <RowDefinition Height="30"/>
                                                                        <RowDefinition Height="30"/>
                                                                        <RowDefinition Height="30"/>
                                                                        <RowDefinition Height="30"/>
                                                                    </Grid.RowDefinitions>
                                                                    <!-- DOR Release -->
                                                                    <TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Text="Double O-Ring Release" FontWeight="Bold" VerticalAlignment="Bottom"/>
                                                                    <Button Grid.Row="0" Grid.Column="1" x:Name="InterlockSave" Content="Save" Width="70" HorizontalAlignment="Left"
                                                    Command="{Binding AlarmSettingSaveCommand}">
                                                                        <Button.Resources>
                                                                            <Style TargetType="Border">
                                                                                <Setter Property="CornerRadius" Value="6"/>
                                                                            </Style>
                                                                        </Button.Resources>
                                                                    </Button>
                                                                    <CheckBox x:Name="cbRecipeNotRunning"  Grid.Row="1" Grid.Column="0" Content="Recipe Not Running" 
                                                      IsChecked="{Binding Path=dInterLockD[RecipeNotRunning]}" VerticalAlignment="Center" Margin="15,0,0,0"/>
                                                                    <CheckBox x:Name="cbReactorPressureLimit" Grid.Row="2" Grid.Column="0" Content="Reactor Pressure(Torr) >="
                                                      IsChecked="{Binding Path=dInterLockA[DORReactorPressure].IsEnable}" VerticalAlignment="Center" Margin="15,0,0,0"/>
                                                                    <TextBox x:Name="tbReactorPressureLimit" Grid.Row="2" Grid.Column="1" Width="150" HorizontalAlignment="Left" Margin="2"
                                                     Text="{Binding Path=dInterLockA[DORReactorPressure].Treshold}"/>
                                                                    <CheckBox x:Name="cbSusceptorTemperature" Grid.Row="3" Grid.Column="0" Content="Susceptor Temperature" 
                                                      IsChecked="{Binding Path=dInterLockA[DORSusceptorTemperature].IsEnable}" VerticalAlignment="Center" Margin="15,0,0,0"/>
                                                                    <TextBox x:Name="DORTemperatureSet" Grid.Row="3" Grid.Column="1" Width="150" HorizontalAlignment="Left" Margin="2"
                                                     Text="{Binding Path=dInterLockA[DORSusceptorTemperature].Treshold}"/>

                                                                    <!-- DOR Lock -->
                                                                    <TextBlock Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="2" Text="Double O-Ring Lock" FontWeight="Bold" VerticalAlignment="Bottom"/>
                                                                    <CheckBox Grid.Row="5" Grid.Column="0" Content=" Vacuum Valve Close"
                                                      IsChecked="{Binding Path=dInterLockD[VacuumValveClose]}" VerticalAlignment="Center" Margin="15,0,0,0"/>
                                                                    <CheckBox Grid.Row="6" Grid.Column="0" Content="Pressure Limit" 
                                                      IsChecked="{Binding Path=dInterLockA[DORPressureLimit].IsEnable}" VerticalAlignment="Center" Margin="15,0,0,0"/>
                                                                    <TextBox x:Name="DORPressureLimit" Grid.Row="6" Grid.Column="1" Width="150" HorizontalAlignment="Left" Margin="2"
                                                     Text="{Binding Path=dInterLockA[DORSusceptorTemperature].Treshold}"/>
                                                                    <CheckBox Grid.Row="7" Grid.Column="0" Content=" Alarm Limit Value" 
                                                      IsChecked="{Binding Path=dInterLockA[AlarmLimit].IsEnable}" VerticalAlignment="Center" Margin="15,0,0,0"/>
                                                                    <TextBox x:Name="DORAlarmLimit" Grid.Row="7" Grid.Column="1" Width="150" HorizontalAlignment="Left" Margin="2"
                                                     Text="{Binding Path=dInterLockA[AlarmLimit].Treshold}"/>
                                                                    <!-- Auto Power ON -->
                                                                    <TextBlock Grid.Row="8" Grid.Column="0" Grid.ColumnSpan="2" Text="Auto Power ON" FontWeight="Bold" VerticalAlignment="Bottom"/>
                                                                    <CheckBox Grid.Row="9" Grid.Column="0" Content=" Induction Power Supply" 
                                                     IsChecked="{Binding Path=dInterLockD[InductionPowerSupply]}" VerticalAlignment="Center" Margin="15,0,0,0"/>
                                                                    <CheckBox Grid.Row="10" Grid.Column="0" Content=" Susceptor Rotation Motor"
                                                     IsChecked="{Binding Path=dInterLockD[SusceptorRotationMotor]}" VerticalAlignment="Center" Margin="15,0,0,0"/>
                                                                    <CheckBox Grid.Row="11" Grid.Column="0" Content=" Reactor Cabinet Lamp" 
                                                      IsChecked="{Binding Path=dInterLockD[ReactorCabinetLamp]}" VerticalAlignment="Center" Margin="15,0,0,0"/>
                                                                    <CheckBox Grid.Row="12" Grid.Column="0" Content=" Gas Control Cabinet Lamp" 
                                                      IsChecked="{Binding Path=dInterLockD[GasControlLamp]}" VerticalAlignment="Center" Margin="15,0,0,0"/>
                                                                </Grid>
                                                            </GroupBox>
                                                            <!-- Preset State 설정 -->
                                                            <GroupBox Header="Preset State" BorderBrush="LightSteelBlue" BorderThickness="2" Margin="2,10,2,0"
                                              FontSize="14">
                                                                <Grid>
                                                                    <Grid.RowDefinitions>
                                                                        <RowDefinition/>
                                                                        <RowDefinition/>
                                                                        <RowDefinition/>
                                                                        <RowDefinition/>
                                                                    </Grid.RowDefinitions>
                                                                    <StackPanel Grid.Row="0" Orientation="Horizontal" Margin="2">
                                                                        <TextBlock Width="200"  Text="System Start State"/>
                                                                        <ComboBox x:Name="comboSystemStart" Width="100" IsReadOnly="True"
                                                          SelectedIndex="{Binding Path=userstate.StartState}"/>
                                                                    </StackPanel>
                                                                    <StackPanel Grid.Row="1" Orientation="Horizontal" Margin="2">
                                                                        <TextBlock Width="200"  Text="Alarm State"/>
                                                                        <ComboBox Name="comboAlarmStart" Width="100"  IsReadOnly="True" 
                                                          SelectedIndex="{Binding Path=userstate.AlarmState}"/>
                                                                    </StackPanel>
                                                                    <StackPanel Grid.Row="2" Orientation="Horizontal" Margin="2">
                                                                        <TextBlock  Width="200" Text="Recipe End"/>
                                                                        <ComboBox Name="comboRecipeEnd" Width="100" IsReadOnly="True" 
                                                          SelectedIndex="{Binding Path=userstate.EndState}" />
                                                                    </StackPanel>
                                                                </Grid>
                                                            </GroupBox>
                                                        </StackPanel>
                                                    </StackPanel>
                                                </Grid>
                                            </Grid>
                                        </TabItem.Content>
                                    </TabItem>
                                    <!-- Tab for Device Setting -->
                                    <TabItem Name="DeviceSetting" Header="Device Setting">
                                        <TabItem.Content>
                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="30"/>
                                                    <RowDefinition />
                                                </Grid.RowDefinitions>
                                                <Grid Grid.Row="0" Background="Blue">
                                                    <TextBlock FontSize="14" Text="Device Setting" FontWeight="Bold" Foreground="White"
                                           VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Grid>
                                                <Grid Grid.Row="1">
                                                    <StackPanel Orientation="Horizontal">
                                                        <!-- Device Name 설정 테이블-->
                                                        <GroupBox Header="Device Naming" Width="620"
                                          BorderBrush="LightSteelBlue" BorderThickness="2" Margin="2,10,2,0"
                                          FontSize="14">
                                                            <StackPanel>
                                                                <Button x:Name="DeviceNameSave" Content="Save" Width="70" HorizontalAlignment="Right" Margin="0,0,50,0">
                                                                    <Button.Resources>
                                                                        <Style TargetType="Border">
                                                                            <Setter Property="CornerRadius" Value="6"/>
                                                                        </Style>
                                                                    </Button.Resources>
                                                                </Button>
                                                                <Grid Height="670">
                                                                    <Grid.RowDefinitions>
                                                                        <RowDefinition Height="50*"/>
                                                                        <RowDefinition Height="Auto"/>
                                                                        <RowDefinition Height="50*"/>
                                                                    </Grid.RowDefinitions>
                                                                    <!-- Analog Device Name 설정 테이블-->
                                                                    <StackPanel>
                                                                        <DataGrid x:Name="MaxValueSetting" AutoGenerateColumns="False" Margin="5,0,5,0">
                                                                            <DataGrid.Resources>
                                                                                <Style TargetType="{x:Type DataGridColumnHeader}">
                                                                                    <Setter Property="HorizontalAlignment" Value="Center"/>
                                                                                    <Setter Property="Background" Value="Transparent"/>
                                                                                </Style>
                                                                                <Style TargetType="{x:Type DataGrid}">
                                                                                    <Setter Property="BorderBrush" Value="Black"/>
                                                                                    <Setter Property="BorderThickness" Value="1"/>
                                                                                </Style>
                                                                            </DataGrid.Resources>
                                                                            <DataGrid.Columns>
                                                                                <DataGridTextColumn Header="Device ID" CanUserSort="False" CanUserResize="False" Width="60"
                                                                        Binding="{Binding}"/>
                                                                                <DataGridTextColumn Header="Device Name" CanUserSort="False" CanUserResize="False" Width="*"
                                                                        Binding="{Binding}"/>
                                                                                <DataGridTextColumn Header="Max Value" CanUserSort="False" CanUserResize="False" Width="120"
                                                                        Binding="{Binding}"/>
                                                                                <DataGridTextColumn Header="Ramping" CanUserSort="False" CanUserResize="False" Width="60"
                                                                        Binding="{Binding}"/>
                                                                                <DataGridTextColumn Header="Accuracy" CanUserSort="False" CanUserResize="False" Width="60"
                                                                        Binding="{Binding}"/>
                                                                                <DataGridTextColumn Header="Units" CanUserSort="False" CanUserResize="False" Width="60"
                                                                        Binding="{Binding}"/>
                                                                            </DataGrid.Columns>
                                                                        </DataGrid>
                                                                    </StackPanel>
                                                                    <GridSplitter Grid.Row="1" HorizontalAlignment="Stretch" VerticalAlignment="Center"  Background="LightSteelBlue"
                                                      ShowsPreview="True" Height="3"/>
                                                                    <!-- Digital Device Name 설정 테이블-->
                                                                    <StackPanel Grid.Row="2" Orientation="Horizontal">
                                                                        <StackPanel>
                                                                            <DataGrid x:Name="DigitalMappingSetting" Width="405" AutoGenerateColumns="False" Margin="5,0,5,0">
                                                                                <DataGrid.Resources>
                                                                                    <Style TargetType="{x:Type DataGridColumnHeader}">
                                                                                        <Setter Property="HorizontalAlignment" Value="Center"/>
                                                                                        <Setter Property="Background" Value="Transparent"/>
                                                                                    </Style>
                                                                                    <Style TargetType="{x:Type DataGrid}">
                                                                                        <Setter Property="BorderBrush" Value="Black"/>
                                                                                        <Setter Property="BorderThickness" Value="1"/>
                                                                                    </Style>
                                                                                </DataGrid.Resources>
                                                                                <DataGrid.Columns>
                                                                                    <DataGridTextColumn Header="Device ID" CanUserSort="False" CanUserResize="False" Width="60"
                                                                                    Binding="{Binding}"/>
                                                                                    <DataGridTextColumn Header="Device Name" CanUserSort="False" CanUserResize="False" Width="*"
                                                                                    Binding="{Binding}"/>
                                                                                    <DataGridTextColumn Header="Sol. ID" CanUserSort="False" CanUserResize="False" Width="120"
                                                                                    Binding="{Binding}"/>
                                                                                </DataGrid.Columns>
                                                                            </DataGrid>
                                                                        </StackPanel>
                                                                        <ListBox x:Name="SolenoidValveID" Width="190">
                                                                            <!-- 솔레노이드밸브 리스트 추가-->
                                                                        </ListBox>

                                                                    </StackPanel>
                                                                </Grid>
                                                            </StackPanel>
                                                        </GroupBox>
                                                        <!-- Component I/O List 테이블-->
                                                        <GroupBox Header="Component I/O List" Width="660"
                                          BorderBrush="LightSteelBlue" BorderThickness="2" Margin="2,10,2,0"
                                          FontSize="14">
                                                            <StackPanel>
                                                                <DataGrid x:Name="IOList" AutoGenerateColumns="False" Margin="5,0,5,0">
                                                                    <DataGrid.Resources>
                                                                        <Style TargetType="{x:Type DataGridColumnHeader}">
                                                                            <Setter Property="HorizontalAlignment" Value="Center"/>
                                                                            <Setter Property="Background" Value="Transparent"/>
                                                                        </Style>
                                                                        <Style TargetType="{x:Type DataGrid}">
                                                                            <Setter Property="BorderBrush" Value="Black"/>
                                                                            <Setter Property="BorderThickness" Value="1"/>
                                                                        </Style>
                                                                    </DataGrid.Resources>
                                                                    <DataGrid.Columns>
                                                                        <DataGridTextColumn Header="ID" CanUserSort="False" CanUserResize="False" Width="60"
                                                                Binding="{Binding}"/>
                                                                        <DataGridTextColumn Header="Name" CanUserSort="False" CanUserResize="False" Width="*"
                                                                Binding="{Binding}"/>
                                                                        <DataGridTextColumn Header="Component" CanUserSort="False" CanUserResize="False" Width="120"
                                                                Binding="{Binding}"/>
                                                                        <DataGridTextColumn Header="Type" CanUserSort="False" CanUserResize="False" Width="60"
                                                                Binding="{Binding}"/>
                                                                        <DataGridTextColumn Header="Signal" CanUserSort="False" CanUserResize="False" Width="60"
                                                                Binding="{Binding}"/>
                                                                        <DataGridTextColumn Header="Terminal" CanUserSort="False" CanUserResize="False" Width="100"
                                                                Binding="{Binding}"/>
                                                                        <DataGridTextColumn Header="Pin No." CanUserSort="False" CanUserResize="False" Width="100"
                                                                Binding="{Binding}"/>
                                                                        <DataGridTextColumn Header="Org.Value" CanUserSort="False" CanUserResize="False" Width="60"
                                                                Binding="{Binding}"/>
                                                                        <DataGridTextColumn Header="Cal. Value" CanUserSort="False" CanUserResize="False" Width="60"
                                                                Binding="{Binding}"/>
                                                                    </DataGrid.Columns>
                                                                </DataGrid>

                                                            </StackPanel>
                                                        </GroupBox>
                                                    </StackPanel>
                                                </Grid>
                                            </Grid>
                                        </TabItem.Content>
                                    </TabItem>
                                    <!-- Tab for I/O List -->
                                    <TabItem Name="IO_List" Header="I/O List">
                                        <TabItem.Content>
                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="30"/>
                                                    <RowDefinition/>
                                                </Grid.RowDefinitions>
                                                <Grid Grid.Row="0" Background="Yellow">
                                                    <TextBlock FontSize="14" Text="I/O List" FontWeight="Bold" Foreground="Black"
                                           VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Grid>
                                                <Grid Grid.Row="1">
                                                    <DataGrid Name="IO_ListTable"
                              Padding="5">
                                                    </DataGrid>
                                                </Grid>
                                            </Grid>
                                        </TabItem.Content>
                                    </TabItem>
                                </TabControl>
                            </Grid>
                        </TabItem>
                    </TabControl>
                </Border>
                <Border Grid.Row="1" Background="{DynamicResource BottomBackgroundColor}">
                </Border>
                <Grid Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="800"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="20"/>
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <!-- Signal PLOT 머리말 Title -->
                    <Grid Grid.Column="0" Background="LightGray" Grid.Row="0">
                        <StackPanel Orientation="Horizontal">
                            <StackPanel.Resources>
                                <Style TargetType="{x:Type Button}">
                                    <Setter Property="Foreground" Value="Black"/>
                                    <Setter Property="Width" Value="76"/>
                                    <Setter Property="Margin" Value="3,0,3,0"/>
                                    <Setter Property="HorizontalAlignment" Value="Center"/>
                                    <Setter Property="VerticalAlignment" Value="Center"/>
                                    <Style.Resources>
                                        <Style TargetType="{x:Type Border}">
                                            <Setter Property="CornerRadius" Value="8"/>
                                        </Style>
                                    </Style.Resources>
                                </Style>
                            </StackPanel.Resources>
                            <TextBlock Width="150" FontSize="14" Text="SIGNAL PLOT" Margin="20,0,0,0"/>
                            <Button x:Name="BtnRecipeLive" Content="RecipeLive" Background="AliceBlue" 
											Command="{Binding BtnRecipeLiveVisibleCommand}"/>
                            <Button x:Name="BtnRecipeProgress" Content="RecipeProgress" Background="AliceBlue"
											Command="{Binding BtnRecipeProgressVisibleCommand}"/>
                            <Button x:Name="BtnPlotAnalog" Content="Analog" Background="Yellow" Margin="100,0,0,0"
											Command="{Binding BtnPlotAnalogCommand}"/>
                            <Button x:Name="BtnPlotTemperatue" Content="Temperature" Background="red"
											Command="{Binding BtnPlotTemperatureCommand}"/>
                            <Button x:Name="BtnPlotPessure" Content="Pressure" Background="Green"
											Command="{Binding BtnPlotPressureCommand}"/>
                            <Button x:Name="BtnPlotTotalFlow" Content="TotalFlow" Background="SkyBlue"
											Command="{Binding BtnPlotTotalFlowCommand}"/>
                        </StackPanel>
                    </Grid>
                    <!-- 그래프 그리는 영역  background="##0f9ed5" -->
                    <Grid Grid.Column="0" Background="White" Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="57*"/>
                            <ColumnDefinition Width="7*"/>
                            <ColumnDefinition Width="29*"/>
                            <ColumnDefinition Width="13*"/>
                            <ColumnDefinition Width="54*"/>
                        </Grid.ColumnDefinitions>
                        <TabControl x:Name="tabChart" Grid.ColumnSpan="5" >
                            <Grid>
                                <!-- Recipe Live chart -->
                                <oxy:PlotView Name="RecipeLive"
															Visibility="{Binding btnRecipeLiveVisible}"
															Model="{Binding RecipeLivePlot}">
                                </oxy:PlotView>
                                <!-- Recipe Progress chart-->
                                <oxy:PlotView Name="RecipeProgress"
															Visibility="{Binding btnRecipeProgressVisible}"
															Model="{Binding RecipeProgressPlot}">
                                </oxy:PlotView>
                            </Grid>
                        </TabControl>

                    </Grid>
                    <!-- 이벤트 로그 머리말 Title -->
                    <Grid Grid.Column="1" Grid.Row="0" Background="LightGray" Margin="2,0,2,0">
                        <StackPanel Orientation="Horizontal">
                            <StackPanel.Resources>
                                <Style TargetType="{x:Type Button}">
                                    <Setter Property="Foreground" Value="White"/>
                                    <Setter Property="Width" Value="76"/>
                                    <Setter Property="Margin" Value="3,0,3,0"/>
                                    <Setter Property="HorizontalAlignment" Value="Center"/>
                                    <Setter Property="VerticalAlignment" Value="Center"/>
                                    <Style.Resources>
                                        <Style TargetType="{x:Type Border}">
                                            <Setter Property="CornerRadius" Value="8"/>
                                        </Style>
                                    </Style.Resources>

                                </Style>
                            </StackPanel.Resources>
                            <TextBlock Width="150" Foreground="Black" FontSize="14" Text="EVENT LOG" Margin="20,0,0,0"/>
                            <Button x:Name="FilterAlarm" Content="Alarm" Background="OrangeRed"/>
                            <Button x:Name="FilterWarning" Content="Waring"  Background="Orange"/>
                            <Button x:Name="FilterInformation" Content="Information" Background="LimeGreen"/>
                            <Button x:Name="FilterClear" Content="Clear" Background="DarkGray"/>
                        </StackPanel>
                    </Grid>
                    <Grid Grid.Row="1" Grid.Column="1">
                        <Grid.Resources>
                            <Style TargetType="DataGrid">
                                <Setter Property="HorizontalGridLinesBrush" Value="red"/>
                                <Setter Property="VerticalGridLinesBrush" Value="Blue" />
                            </Style>
                            <Style TargetType="DataGridColumnHeader">
                                <Setter Property="HorizontalContentAlignment" Value="Center"/>
                                <Setter Property="BorderBrush" Value="blue" />
                                <Setter Property="BorderThickness" Value="1" />
                                <Setter Property="Background" Value="Yellow" />
                                <Setter Property="SeparatorVisibility" Value="Visible" />
                            </Style>
                            <Style TargetType="DataGridCell">
                                <Style.Triggers>
                                    <Trigger Property="IsSelected" Value="True">
                                        <Setter Property="Background" Value="Beige"/>
                                        <Setter Property="Foreground" Value="Brown"/>
                                    </Trigger>
                                </Style.Triggers>
                            </Style>
                        </Grid.Resources>
                        <DataGrid x:Name="dataGrid" AutoGenerateColumns="True" SelectionMode="Extended"
											CanUserAddRows="False" AlternationCount="2" AlternatingRowBackground="LightGray"
											ItemsSource="{Binding EventData}">
                            <DataGrid.Resources>

                            </DataGrid.Resources>
                        </DataGrid>
                    </Grid>

                </Grid>

            </Grid>
            <!-- Start: GridColumn Right -->
            <Grid Grid.Column="2" x:Name="gridMenuRight">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="240"/>
                </Grid.RowDefinitions>
                <Border Grid.Row="0" Background="{DynamicResource SideBackgroundColor}">
                </Border>
                <Grid Grid.Row="0">
                    <StackPanel>
                        <StackPanel.Resources>
                            <Style TargetType="TextBlock">
                                <Setter Property="Foreground" Value="{DynamicResource PrimaryFontColor}"/>
                                <Setter Property="FontFamily" Value="Pretendard"/>
                                <Setter Property="FontSize" Value="14"/>
                                <Setter Property="Width" Value="200"/>
                                <Setter Property="Margin" Value="20,2,0,2"/>
                            </Style>
                            <Style TargetType="TextBox">
                                <Setter Property="Foreground" Value="{DynamicResource PrimaryFontColor}"/>
                                <Setter Property="FontFamily" Value="Pretendard"/>
                                <Setter Property="FontSize" Value="14"/>
                            </Style>
                            <Style TargetType="Ellipse">
                                <Setter Property="Width" Value="10"/>
                                <Setter Property="Height" Value="10"/>
                                <Setter Property="Margin" Value="10,0,0,0"/>
                                <Setter Property="Fill" Value="LawnGreen"/>
                            </Style>
                        </StackPanel.Resources>
                        <!-- Current State List -->
                        <StackPanel Height="200" VerticalAlignment="Top">
                            <TextBlock HorizontalAlignment="Center" Margin="2">
                            <Bold>Current State</Bold></TextBlock>
                            <Line X1="5" Y1="0" X2="295" Y2="0" Stroke="DarkBlue" StrokeThickness="2"/>
                            <StackPanel Orientation="Horizontal">
                                <Button x:Name="GasState" FontSize="14" Content="Idle State" Width="100" Margin="10,1,0,1"/>
                                <TextBox x:Name="GasCurrentValue" Width="100" Margin="10,1,0,1">
                                    100
                                </TextBox>
                                <Ellipse x:Name="GasCurrentState"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <Button x:Name="ChamberState" FontSize="14" Content="Chamber Open" Width="100" Margin="10,1,0,1"/>
                                <TextBox x:Name="ChamberCurrentValue" FontSize="14" Width="100" Margin="10,1,0,1">
                                    100
                                </TextBox>
                                <Ellipse x:Name="ChamberCurrentState" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <Button x:Name="LeakState" FontSize="14" Content="Leak Test" Width="100" Margin="10,1,0,1"/>
                                <TextBox x:Name="LeakCurrentValue" FontSize="14" Width="100" Margin="10,1,0,1">
                                    Leak Test Mode
                                </TextBox>
                                <Ellipse x:Name="LeakCurrentState" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <Button x:Name="SoftStopState" FontSize="14" Content="Soft STOP" Width="100" Margin="10,1,0,1"/>
                                <TextBox x:Name="SoftStopValue" FontSize="14" Width="100" Margin="10,1,0,1">
                                    50
                                </TextBox>
                                <Ellipse x:Name="SoftStopCurrentState" />
                            </StackPanel>
                        </StackPanel>
                        <!-- Enable State List -->
                        <StackPanel Height="230" VerticalAlignment="Top">
                            <TextBlock HorizontalAlignment="Center" Margin="2" FontWeight="Bold" Text="Enable State"/>
                            <Line X1="5" Y1="0" X2="295" Y2="0" Stroke="DarkBlue" StrokeThickness="2"/>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Recipe Allowed"/>
                                <Ellipse x:Name="RecipeAllowedState"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Control Allowed"/>
                                <Ellipse x:Name="ControlAllowedState"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="H2 Gas Supply"/>
                                <Ellipse x:Name="H2GasSupplyState" Fill="Red"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Hydride Gas Supply"/>
                                <Ellipse x:Name="HydrideGasSupplyState" Fill="Red"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Reactor Open Enable"/>
                                <Ellipse x:Name="ReactorOpenState"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Reactor Open Timer"/>
                                <Ellipse x:Name="ReactorOpenTime"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Heating Allowed"/>
                                <Ellipse x:Name="HeatingAllowedState"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Text="Double O-Ring"/>
                                <Ellipse x:Name="DORState"/>
                            </StackPanel>
                        </StackPanel>
                        <!-- Utility State -->
                        <StackPanel Height="200" VerticalAlignment="Top">
                            <TextBlock Foreground="DarkBlue" FontSize="14" HorizontalAlignment="Center" Margin="2">
                        <Bold>Utility State</Bold></TextBlock>
                            <Line X1="5" Y1="0" X2="295" Y2="0" Stroke="DarkBlue" StrokeThickness="2"/>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Pneumatic Pressure</TextBlock>
                                <Ellipse x:Name="PneumaticPressureState" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Cooling Water</TextBlock>
                                <Ellipse x:Name="CoolingWaterState" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Reactor Cabinet Door</TextBlock>
                                <Ellipse x:Name="ReactorDoorState" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Gas Cabinet Door</TextBlock>
                                <Ellipse x:Name="GasDoorState" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Control Cabinet Door</TextBlock>
                                <Ellipse x:Name="ControlDoorState" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                        </StackPanel>
                        <!-- Alarm State -->
                        <StackPanel  VerticalAlignment="Top">
                            <TextBlock Foreground="DarkBlue" FontSize="14" HorizontalAlignment="Center" Margin="2">
                        <Bold>Equipment State</Bold></TextBlock>
                            <Line X1="5" Y1="0" X2="295" Y2="0" Stroke="DarkBlue" StrokeThickness="2"/>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Alarm</TextBlock>
                                <Ellipse x:Name="AlarmState" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Warning</TextBlock>
                                <Ellipse x:Name="WarningState" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Silence</TextBlock>
                                <Ellipse x:Name="SilenceState" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="170">User</TextBlock>
                                <TextBox Width="100">Administartor</TextBox>
                            </StackPanel>
                        </StackPanel>
                    </StackPanel>
                </Grid>
                <Border Grid.Row="1" Background="{DynamicResource PrimaryBackgroundColor}"
                        BorderBrush="Black" BorderThickness="1">
                </Border>
                <!-- Device Information Block -->
                <Grid Grid.Row="1" >
