<Window x:Class="SapphireXE_App.Views.MainView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
				xmlns:b="http://schemas.microsoft.com/xaml/behaviors"
				xmlns:oxy="http://oxyplot.org/wpf"
				xmlns:control="clr-namespace:SapphireXE_App.Controls"
				mc:Ignorable="d"
        Title="Sapphire XE" Height="1080" Width="1920"
        WindowStartupLocation="CenterScreen"
        ResizeMode="CanResizeWithGrip" WindowState="Normal" WindowStyle="None"
				MouseLeftButtonDown="Window_MouseLeftButtonDown"
				MouseRightButtonUp="Window_MouseRightButtonUp">

    <Border CornerRadius="8" Background="{DynamicResource PrimaryBackgroundColor}">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="300"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="300"/>
            </Grid.ColumnDefinitions>
            <!-- Start: GridColumn Left -->
            <Grid Grid.Column="0" x:Name="gridControlRight">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="240"/>
                </Grid.RowDefinitions>
                <Border Grid.Row="0" Background="{DynamicResource SideBackgroundColor}">
                </Border>
                <Border Grid.Row="1" Background="{DynamicResource SideBackgroundColor}">
                </Border>
                <Grid Grid.Row="0">
                    <StackPanel>
                        <Panel.Resources>
                            <Style TargetType="TextBlock">
                                <Setter Property="Foreground" Value="{DynamicResource PrimaryFontColor}"/>
                                <Setter Property="FontFamily" Value="Pretendard"/>
                                <Setter Property="FontSize" Value="14"/>
                                <Setter Property="Margin" Value="0,2,0,2"/>
                                <Setter Property="Width" Value="200"/>
                            </Style>
                        </Panel.Resources>
                        <!-- Machine Information -->
                        <StackPanel Height="160">
                            <TextBlock Margin="10,20,0,2" Text="Machine Name : Marvel 180R"/>
                            <TextBlock Text="Application : GaN"/>
                            <TextBlock Text="System ID : 2024"/>
                            <TextBlock Text="{Binding plcMode}"/>
                            <TextBlock Text="{Binding plcAddress}"/>
                        </StackPanel>
                        <!-- Heater Control Button -->
                        <StackPanel Height="100">
                            <TextBlock Foreground="DarkBlue" FontSize="14" HorizontalAlignment="Center" Margin="2">
                                <Bold>Heater Control</Bold></TextBlock>
                            <Line X1="5" Y1="0" X2="295" Y2="0" Stroke="DarkBlue" StrokeThickness="2"/>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition />
                                    <ColumnDefinition />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <TextBlock FontSize="14" Grid.Column="0" Grid.Row="0" HorizontalAlignment="Center" Text="SP" Margin="2"/>
                                <TextBlock FontSize="14" Grid.Column="1" Grid.Row="0" HorizontalAlignment="Center" Text="Temp." Margin="2"/>
                                <TextBlock FontSize="14" Grid.Column="2" Grid.Row="0" HorizontalAlignment="Center" Text="Power(%)" Margin="2"/>
                                <TextBox  Width="80" FontSize="14" Grid.Column="0" Grid.Row="1"
													HorizontalAlignment="Center"
													VerticalContentAlignment="Center"
													HorizontalContentAlignment="Center">
                                    1,000
                                </TextBox>
                                <TextBox Width="80" FontSize="14" Grid.Column="1" Grid.Row="1"
												 HorizontalAlignment="Center" Margin="2" 
												 VerticalContentAlignment="Center"
												 HorizontalContentAlignment="Center">
                                    1,000
                                </TextBox>
                                <TextBox Width="80" FontSize="14" Grid.Column="2" Grid.Row="1"
												 HorizontalAlignment="Center" Margin="2" 
												 VerticalContentAlignment="Center"
												 HorizontalContentAlignment="Center">
                                    30
                                </TextBox>
                                <TextBlock FontSize="14" Grid.ColumnSpan="2" Grid.Row="2">Heater Control Mode :</TextBlock>
                                <Button FontSize="14" Grid.Column="2" Grid.Row="2" Margin="5,2,10,2">Power Rate</Button>
                            </Grid>
                        </StackPanel>
                        <!-- Pressure Control Button -->
                        <StackPanel Height="100">
                            <TextBlock Foreground="DarkBlue" FontSize="14" HorizontalAlignment="Center" Margin="2">
                                <Bold>Pressure Control</Bold></TextBlock>
                            <Line X1="5" Y1="0" X2="295" Y2="0" Stroke="DarkBlue" StrokeThickness="2"/>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition />
                                    <ColumnDefinition />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <TextBlock FontSize="14" Grid.Column="0" Grid.Row="0" HorizontalAlignment="Center" Text="SP" Margin="2"/>
                                <TextBlock FontSize="14" Grid.Column="1" Grid.Row="0" HorizontalAlignment="Center" Text="Pressure" Margin="2"/>
                                <TextBlock FontSize="14" Grid.Column="2" Grid.Row="0" HorizontalAlignment="Center" Text="Position(%)" Margin="2"/>
                                <TextBox  Width="80" FontSize="14" Grid.Column="0" Grid.Row="1" HorizontalAlignment="Center" 
													VerticalContentAlignment="Center"	HorizontalContentAlignment="Center">
                                    760
                                </TextBox>
                                <TextBox Width="80" FontSize="14" Grid.Column="1" Grid.Row="1" HorizontalAlignment="Center" Margin="2"
												 VerticalContentAlignment="Center" HorizontalContentAlignment="Center">
                                    760
                                </TextBox>
                                <TextBox Width="80" FontSize="14" Grid.Column="2" Grid.Row="1" HorizontalAlignment="Center" Margin="2"
												 VerticalContentAlignment="Center" HorizontalContentAlignment="Center">
                                    10
                                </TextBox>
                                <TextBlock FontSize="14" Grid.ColumnSpan="2" Grid.Row="2">Heater Control Mode :</TextBlock>
                                <Button FontSize="14" Grid.Column="2" Grid.Row="2" Margin="5,2,10,2">Position(%)</Button>
                            </Grid>
                        </StackPanel>
                        <!-- Rotation Control Button -->
                        <StackPanel Height="100">
                            <TextBlock Foreground="DarkBlue" FontSize="14" HorizontalAlignment="Center" Margin="2">
								<Bold>Rotation Control</Bold>
                            </TextBlock>
                            <Line X1="5" Y1="0" X2="295" Y2="0" Stroke="DarkBlue" StrokeThickness="2"/>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition />
                                    <ColumnDefinition />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <TextBlock FontSize="14" Grid.Column="0" Grid.Row="0" HorizontalAlignment="Center" Text="SP" Margin="2"/>
                                <TextBlock FontSize="14" Grid.Column="1" Grid.Row="0" HorizontalAlignment="Center" Text="Rotation" Margin="2"/>
                                <TextBlock FontSize="14" Grid.Column="2" Grid.Row="0" HorizontalAlignment="Center" Text="Power" Margin="2"/>
                                <TextBox  Width="80" FontSize="14" Grid.Column="0" Grid.Row="1" HorizontalAlignment="Center"
                                           VerticalContentAlignment="Center"
                                          HorizontalContentAlignment="Center">
                                    50
                                </TextBox>
                                <TextBox  Width="80" FontSize="14" Grid.Column="1" Grid.Row="1" HorizontalAlignment="Center"
                                           VerticalContentAlignment="Center"
                                          HorizontalContentAlignment="Center">
                                    50
                                </TextBox>
                                <Button FontSize="14" Grid.Column="2" Grid.Row="2" Margin="5,2,10,2">ON</Button>
                            </Grid>
                        </StackPanel>
                        <!-- Source Status Button -->
                        <StackPanel>
                            <TextBlock Foreground="DarkBlue" FontSize="14" HorizontalAlignment="Center" Margin="2">
                                <Bold>Source States</Bold></TextBlock>
                            <Line X1="5" Y1="0" X2="295" Y2="0" Stroke="DarkBlue" StrokeThickness="2"/>
                            <Grid VerticalAlignment="Top">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="100"/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <TextBlock FontSize="14" Grid.Column="1" Grid.Row="0" Text="Carrier" HorizontalAlignment="Center"/>
                                <TextBlock FontSize="14" Grid.Column="2" Grid.Row="0" Text="Source" HorizontalAlignment="Center"/>
                                <TextBlock FontSize="14" Grid.Column="3" Grid.Row="0" Text="Manifold" HorizontalAlignment="Center"/>
                                <TextBlock FontSize="14" Grid.Column="0" Grid.Row="1" Text="NH3 #1" Margin="2"/>
                                <TextBlock FontSize="14" Grid.Column="0" Grid.Row="2" Text="NH3 #2" Margin="2"/>
                                <TextBlock FontSize="14" Grid.Column="0" Grid.Row="3" Text="SiH4" Margin="2"/>
                                <TextBlock FontSize="14" Grid.ColumnSpan="4" Grid.Row="4" Text="NH3 #1"/>
                                <TextBlock FontSize="14" Grid.Column="0" Grid.Row="5" Text="TEB" Margin="2"/>
                                <TextBlock FontSize="14" Grid.Column="0" Grid.Row="6" Text="TMAl" Margin="2"/>
                                <TextBlock FontSize="14" Grid.Column="0" Grid.Row="7" Text="TMIn" Margin="2"/>
                                <TextBlock FontSize="14" Grid.Column="0" Grid.Row="8" Text="TMGa" Margin="2"/>
                                <TextBlock FontSize="14" Grid.Column="0" Grid.Row="9" Text="DTMGa" Margin="2"/>
                                <TextBlock FontSize="14" Grid.Column="0" Grid.Row="10" Text="Cp2Mg" Margin="2"/>
                                <Button FontSize="14" Grid.Column="1" Grid.Row="1" Margin="2">N2</Button>
                                <Button FontSize="14" Grid.Column="2" Grid.Row="1" Margin="2">OFF</Button>
                                <Button FontSize="14" Grid.Column="3" Grid.Row="1" Margin="2">VENT</Button>
                                <Button FontSize="14" Grid.Column="1" Grid.Row="2" Margin="2">N2</Button>
                                <Button FontSize="14" Grid.Column="2" Grid.Row="2" Margin="2">OFF</Button>
                                <Button FontSize="14" Grid.Column="3" Grid.Row="2" Margin="2">VENT</Button>
                                <Button FontSize="14" Grid.Column="1" Grid.Row="3" Margin="2">N2</Button>
                                <Button FontSize="14" Grid.Column="2" Grid.Row="3" Margin="2">OFF</Button>
                                <Button FontSize="14" Grid.Column="3" Grid.Row="3" Margin="2">VENT</Button>
                                <Button FontSize="14" Grid.Column="1" Grid.Row="4" Margin="2">N2</Button>
                                <Button FontSize="14" Grid.Column="2" Grid.Row="4" Margin="2">OFF</Button>
                                <Button FontSize="14" Grid.Column="3" Grid.Row="4" Margin="2">VENT</Button>
                                <Button FontSize="14" Grid.Column="1" Grid.Row="5" Margin="2">N2</Button>
                                <Button FontSize="14" Grid.Column="2" Grid.Row="5" Margin="2">OFF</Button>
                                <Button FontSize="14" Grid.Column="3" Grid.Row="5" Margin="2">VENT</Button>
                                <Button FontSize="14" Grid.Column="1" Grid.Row="6" Margin="2">N2</Button>
                                <Button FontSize="14" Grid.Column="2" Grid.Row="6" Margin="2">OFF</Button>
                                <Button FontSize="14" Grid.Column="3" Grid.Row="6" Margin="2">VENT</Button>
                                <Button FontSize="14" Grid.Column="1" Grid.Row="7" Margin="2">N2</Button>
                                <Button FontSize="14" Grid.Column="2" Grid.Row="7" Margin="2">OFF</Button>
                                <Button FontSize="14" Grid.Column="3" Grid.Row="7" Margin="2">VENT</Button>
                                <Button FontSize="14" Grid.Column="1" Grid.Row="8" Margin="2">N2</Button>
                                <Button FontSize="14" Grid.Column="2" Grid.Row="8" Margin="2">OFF</Button>
                                <Button FontSize="14" Grid.Column="3" Grid.Row="8" Margin="2">VENT</Button>
                                <Button FontSize="14" Grid.Column="1" Grid.Row="9" Margin="2">N2</Button>
                                <Button FontSize="14" Grid.Column="2" Grid.Row="9" Margin="2">OFF</Button>
                                <Button FontSize="14" Grid.Column="3" Grid.Row="9" Margin="2">VENT</Button>
                                <Button FontSize="14" Grid.Column="1" Grid.Row="10" Margin="2" Background="Red">H2</Button>
                                <Button FontSize="14" Grid.Column="2" Grid.Row="10" Margin="2" Background="LawnGreen">ON</Button>
                                <Button FontSize="14" Grid.Column="3" Grid.Row="10" Margin="2" Background="Magenta">RUN</Button>
                            </Grid>
                        </StackPanel>
                    </StackPanel>
                </Grid>
                <Border Grid.Row="1" BorderBrush="Black" BorderThickness="1">
                </Border>
                <!-- Line Heater Information-->
                <Grid Grid.Row="1">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="60"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="0" Background="LightGray" Margin="1,0,1,0">
                        <TextBlock Foreground="Black" FontSize="14" Text="LINE HEATER" Margin="20,0,0,0"/>
                    </Grid>
                    <Grid Grid.Row="1" Margin="1,0,1,0">
                        <StackPanel Orientation="Vertical">
                            <StackPanel Orientation="Horizontal" Height="30">
                                <TextBlock Foreground="Black" FontSize="12" Text="1 " Height="18" Width="28" TextAlignment="Right"/>
                                <TextBox Width="45" Height="20" VerticalContentAlignment="Center"/>
                                <TextBlock Foreground="Black" FontSize="12" Text="2 " Height="18" Width="28" TextAlignment="Right"/>
                                <TextBox Width="45" Height="20" VerticalContentAlignment="Center"/>
                                <TextBlock Foreground="Black" FontSize="12" Text="3 " Height="18" Width="28" TextAlignment="Right"/>
                                <TextBox Width="45" Height="20" VerticalContentAlignment="Center"/>
                                <TextBlock Foreground="Black" FontSize="12" Text="4 " Height="18" Width="28" TextAlignment="Right"/>
                                <TextBox Width="45" Height="20" VerticalContentAlignment="Center"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Height="30">
                                <TextBlock Foreground="Black" FontSize="12" Text="5 " Height="18" Width="28" TextAlignment="Right"/>
                                <TextBox Width="45" Height="20" VerticalContentAlignment="Center"/>
                                <TextBlock Foreground="Black" FontSize="12" Text="6 " Height="18" Width="28" TextAlignment="Right"/>
                                <TextBox Width="45" Height="20" VerticalContentAlignment="Center"/>
                                <TextBlock Foreground="Black" FontSize="12" Text="7 " Height="18" Width="28" TextAlignment="Right"/>
                                <TextBox Width="45" Height="20" VerticalContentAlignment="Center"/>
                                <TextBlock Foreground="Black" FontSize="12" Text="8 " Height="18" Width="28" TextAlignment="Right"/>
                                <TextBox Width="45" Height="20" VerticalContentAlignment="Center"/>

                            </StackPanel>
                        </StackPanel>
                    </Grid>

                    <Grid Grid.Row="2" Background="LightGray" Margin="1,0,1,0">
                        <TextBlock Foreground="Black" FontSize="14" Text="COOLING WATER TEMP." Margin="20,0,0,0"/>
                    </Grid>
                    <Grid Grid.Row="3">
                        <StackPanel Orientation="Vertical">
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Text=" Shower Head : " Width="150" />
                                <TextBox Width="148"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Text=" Induction Coil : " Width="150" />
                                <TextBox Width="149"/>
                            </StackPanel>
                        </StackPanel>
                    </Grid>
                </Grid>

            </Grid>

            <!-- Start: GridColumn Center -->
            <Grid  Grid.Column="1" x:Name="gridControlCenter">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="240"/>
                </Grid.RowDefinitions>
                <Border Grid.Row="0" >
                    <TabControl x:Name="tabControl" HorizontalAlignment="Stretch" >
                        <!-- Manual Control 화면 -->
                        <TabItem Header="System Control" Width="120">
                            <Canvas Background="{DynamicResource PrimaryBackgroundColor}">
                                <!-- Drawing Nitrogen Gas Line -->
                                <Polyline Points="196,80 196,93 90,93 90,502 1000,502 1000,522" Stroke="Lime" StrokeThickness="2" />
                                <Line X1="0" Y1="0" X2="0" Y2="20" Stroke="Lime" StrokeThickness="2" Canvas.Left="300" Canvas.Top="502" />
                                <Line X1="0" Y1="0" X2="0" Y2="20" Stroke="Lime" StrokeThickness="2" Canvas.Left="440" Canvas.Top="502" />
                                <Line X1="0" Y1="0" X2="0" Y2="20" Stroke="Lime" StrokeThickness="2" Canvas.Left="580" Canvas.Top="502" />
                                <Line X1="0" Y1="0" X2="0" Y2="20" Stroke="Lime" StrokeThickness="2" Canvas.Left="720" Canvas.Top="502" />
                                <Line X1="0" Y1="0" X2="0" Y2="20" Stroke="Lime" StrokeThickness="2" Canvas.Left="860" Canvas.Top="502" />

                                <Line X1="0" Y1="0" X2="20" Y2="0" Stroke="Lime" StrokeThickness="2" Canvas.Left="90" Canvas.Top="115" />
                                <Line X1="0" Y1="0" X2="20" Y2="0" Stroke="Lime" StrokeThickness="2" Canvas.Left="90" Canvas.Top="300" />
                                <!-- Drawing Hydrogen Gas Line -->
                                <Polyline Points="70, 80 70, 490 980, 490 980,522" Stroke="Red" StrokeThickness="2" />
                                <Line X1="0" Y1="0" X2="0" Y2="32" Stroke="Red" StrokeThickness="2" Canvas.Left="280" Canvas.Top="490" />
                                <Line X1="0" Y1="0" X2="0" Y2="32" Stroke="Red" StrokeThickness="2" Canvas.Left="420" Canvas.Top="490" />
                                <Line X1="0" Y1="0" X2="0" Y2="32" Stroke="Red" StrokeThickness="2" Canvas.Left="560" Canvas.Top="490" />
                                <Line X1="0" Y1="0" X2="0" Y2="32" Stroke="Red" StrokeThickness="2" Canvas.Left="700" Canvas.Top="490" />
                                <Line X1="0" Y1="0" X2="0" Y2="32" Stroke="Red" StrokeThickness="2" Canvas.Left="840" Canvas.Top="490" />

                                <Line X1="0" Y1="0" X2="40" Y2="0" Stroke="Red" StrokeThickness="2" Canvas.Left="70" Canvas.Top="135" />
                                <Line X1="0" Y1="0" X2="40" Y2="0" Stroke="Red" StrokeThickness="2" Canvas.Left="70" Canvas.Top="320" />
                                <!-- Drawing Ammonia Gas Line -->
                                <Line X1="320" Y1="80" X2="320" Y2="126" Stroke="Yellow" StrokeThickness="2" />
                                <!-- Drawing Silane Gas Line -->
                                <Line X1="684" Y1="80" X2="684" Y2="126" Stroke="Orange" StrokeThickness="2" />
                                <!-- Drawing Process Gas Line -->
                                <Polyline Points="132,115 664 115 664, 214" Stroke="Magenta" StrokeThickness="2" />
                                <Line X1="0" Y1="0" X2="0" Y2="143" Stroke="Magenta" StrokeThickness="2" Canvas.Left="300" Canvas.Top="115" />
                                <Line X1="0" Y1="0" X2="0" Y2="143" Stroke="Magenta" StrokeThickness="2" Canvas.Left="580" Canvas.Top="115" />
                                <Polyline Points="300, 203 440, 203 440, 258" Stroke="Magenta" StrokeThickness="2" />
                                <Line X1="580" Y1="203" X2="664" Y2="203" Stroke="Magenta" StrokeThickness="2" />
                                <Line X1="132" Y1="300" X2="182" Y2="300" Stroke="Magenta" StrokeThickness="2" />
                                <!-- 샤워헤드 IN Arrow  -->
                                <Path Canvas.Left="1067" Canvas.Top="322" Fill="Magenta">
                                    <Path.Data>
                                        <PathGeometry>
                                            <PathFigure StartPoint="0 0" IsClosed="True">
                                                <LineSegment Point="-10 -5"/>
                                                <LineSegment Point="-10 5"/>
                                            </PathFigure>
                                        </PathGeometry>
                                    </Path.Data>
                                </Path>

                                <Path Canvas.Left="1067" Canvas.Top="366" Fill="Magenta">
                                    <Path.Data>
                                        <PathGeometry>
                                            <PathFigure StartPoint="0 0" IsClosed="True">
                                                <LineSegment Point="-10 -5"/>
                                                <LineSegment Point="-10 5"/>
                                            </PathFigure>
                                        </PathGeometry>
                                    </Path.Data>
                                </Path>

                                <Line X1="160" Y1="322" X2="1065" Y2="322" Stroke="Magenta" StrokeThickness="2" />
                                <Polyline Points="160, 344 750, 344 750, 242 1060, 242" Stroke="Magenta" StrokeThickness="2" />
                                <Polyline Points="750, 270 1100, 270 1100, 260" Stroke="Magenta" StrokeThickness="2" />
                                <Line X1="160" Y1="366" X2="1065" Y2="366" Stroke="Magenta" StrokeThickness="2" />
                                <Line X1="160" Y1="388" X2="182" Y2="388" Stroke="Magenta" StrokeThickness="2" />
                                <Line X1="160" Y1="300" X2="160" Y2="388" Stroke="Magenta" StrokeThickness="2" />
                                <Line X1="1100" Y1="107" X2="1100" Y2="120" Stroke="Magenta" StrokeThickness="2" />

                                <!-- Reactor Inlet 화살표 A -->
                                <Path Canvas.Left="1100" Canvas.Top="110" Fill="Magenta">
                                    <Path.Data>
                                        <PathGeometry>
                                            <PathFigure StartPoint="0 0" IsClosed="True">
                                                <LineSegment Point="4 -4"/>
                                                <LineSegment Point="4 -12"/>
                                                <LineSegment Point="0 -8"/>
                                                <LineSegment Point="-4 -12"/>
                                                <LineSegment Point="-4 -4"/>
                                            </PathFigure>
                                        </PathGeometry>
                                    </Path.Data>
                                </Path>
                                <!-- Reactor Inlet 화살표 B -->
                                <Path Canvas.Left="1160" Canvas.Top="110" Fill="Magenta">
                                    <Path.Data>
                                        <PathGeometry>
                                            <PathFigure StartPoint="0 0" IsClosed="True">
                                                <LineSegment Point="4 -4"/>
                                                <LineSegment Point="4 -12"/>
                                                <LineSegment Point="0 -8"/>
                                                <LineSegment Point="-4 -12"/>
                                                <LineSegment Point="-4 -4"/>
                                            </PathFigure>
                                        </PathGeometry>
                                    </Path.Data>
                                </Path>

                                <Line X1="1160" Y1="107" X2="1160" Y2="120" Stroke="Magenta" StrokeThickness="2" />

                                <Line X1="0" Y1="0" X2="0" Y2="42" Stroke="Magenta" StrokeThickness="2" Canvas.Left="320" Canvas.Top="280"/>
                                <Line X1="0" Y1="0" X2="0" Y2="42" Stroke="Magenta" StrokeThickness="2" Canvas.Left="460" Canvas.Top="280"/>
                                <Line X1="0" Y1="0" X2="0" Y2="42" Stroke="Magenta" StrokeThickness="2" Canvas.Left="600" Canvas.Top="280"/>

                                <Line X1="0" Y1="0" X2="0" Y2="42" Stroke="Magenta" StrokeThickness="2" Canvas.Left="300" Canvas.Top="366"/>
                                <Line X1="0" Y1="0" X2="0" Y2="42" Stroke="Magenta" StrokeThickness="2" Canvas.Left="440" Canvas.Top="366"/>
                                <Line X1="0" Y1="0" X2="0" Y2="42" Stroke="Magenta" StrokeThickness="2" Canvas.Left="580" Canvas.Top="366"/>
                                <Line X1="0" Y1="0" X2="0" Y2="42" Stroke="Magenta" StrokeThickness="2" Canvas.Left="720" Canvas.Top="366"/>
                                <Line X1="0" Y1="0" X2="0" Y2="42" Stroke="Magenta" StrokeThickness="2" Canvas.Left="860" Canvas.Top="366"/>
                                <Line X1="0" Y1="0" X2="0" Y2="42" Stroke="Magenta" StrokeThickness="2" Canvas.Left="1000" Canvas.Top="366"/>

                                <Line X1="0" Y1="0" X2="0" Y2="160" Stroke="Magenta" StrokeThickness="2" Canvas.Left="300" Canvas.Top="544"/>
                                <Line X1="0" Y1="0" X2="0" Y2="274" Stroke="Magenta" StrokeThickness="2" Canvas.Left="320" Canvas.Top="430"/>
                                <Line X1="0" Y1="0" X2="0" Y2="160" Stroke="Magenta" StrokeThickness="2" Canvas.Left="440" Canvas.Top="544"/>
