                                <!-- 샤워헤드 IN Arrow  -->
                                <Path Canvas.Left="1067" Canvas.Top="322" Fill="Magenta">
                                    <Path.Data>
                                        <PathGeometry>
                                            <PathFigure StartPoint="0 0" IsClosed="True">
                                                <LineSegment Point="-10 -5"/>
                                                <LineSegment Point="-10 5"/>
                                            </PathFigure>
                                        </PathGeometry>
