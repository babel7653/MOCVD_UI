                                                        <Style x:Key="Content2" TargetType="{x:Type TextBlock}">
                                                            <Setter Property="Foreground" Value="Black"/>
                                                            <Setter Property="FontSize" Value="12"/>
                                                            <Setter Property="VerticalAlignment" Value="Center"/>
                                                        </Style>
                                                    </DataGrid.Resources>

                                                    <DataGrid.Columns>
                                                        <DataGridTextColumn Header="No." Binding="{Binding RecipeStep}" ElementStyle="{StaticResource Content1}" Width="30"/>
                                                        <DataGridTextColumn Header="Name" Binding="{Binding RecipeName}" ElementStyle="{StaticResource Content2}" Width="150"/>
                                                        <DataGridTextColumn Header="Ramp" Binding="{Binding RecipeRampingTime}" ElementStyle="{StaticResource Content2}" Width="60"/>
                                                        <DataGridTextColumn Header="Hold" Binding="{Binding RecipeHoldingTime}" ElementStyle="{StaticResource Content2}" Width="60"/>
                                                        <DataGridTextColumn Header="Loop" Binding="{Binding RecipeLoop}" ElementStyle="{StaticResource Content1}" Width="60"/>
                                                        <DataGridTextColumn Header="Jump" Binding="{Binding RecipeJump}" ElementStyle="{StaticResource Content1}" Width="60"/>
                                                    </DataGrid.Columns>
                                                </DataGrid>
                                            </TabItem.Content>
                                        </TabItem>

                                    </TabControl>
                                </Grid>
                            </Grid>
                        </TabItem>
                        <!-- Report 화면 -->
                        <TabItem Header="Report" Width="120">
                            <Canvas Background="DimGray">

                            </Canvas>

                        </TabItem>
                        <!-- Setting 화면 -->
                        <TabItem Header="Setting" Width="120">
                            <Grid Background="DimGray"/>
                        </TabItem>
                    </TabControl>
                </Border>
                <Border Grid.Row="1" Background="{DynamicResource BottomBackgroundColor}">
                </Border>
                <Grid Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="800"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="20"/>
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <!-- Signal PLOT 머리말 Title -->
                    <Grid Grid.Column="0" Background="LightGray" Grid.Row="0">
                        <StackPanel Orientation="Horizontal">
                            <StackPanel.Resources>
                                <Style TargetType="{x:Type Button}">
                                    <Setter Property="Foreground" Value="Black"/>
                                    <Setter Property="Width" Value="76"/>
                                    <Setter Property="Margin" Value="3,0,3,0"/>
                                    <Setter Property="HorizontalAlignment" Value="Center"/>
                                    <Setter Property="VerticalAlignment" Value="Center"/>
                                    <Style.Resources>
                                        <Style TargetType="{x:Type Border}">
                                            <Setter Property="CornerRadius" Value="8"/>
                                        </Style>
                                    </Style.Resources>
                                </Style>
                            </StackPanel.Resources>
                            <TextBlock Width="400" FontSize="14" Text="SIGNAL PLOT" Margin="20,0,0,0"/>
                            <Button x:Name="PlotAnalog" Content="Analog" Background="Yellow" />
                            <Button x:Name="PlotTemperatue" Content="Temperature" Background="red"/>
                            <Button x:Name="PlotPessure" Content="Pressure" Background="Green"/>
                            <Button x:Name="PlotTotalFlow" Content="TotalFlow" Background="SkyBlue"/>
                        </StackPanel>
                    </Grid>
                    <!-- 그래프 그리는 영역 -->
                    <Grid Grid.Column="0" Background="#0f9ed5" Grid.Row="1">
                        <lvc:CartesianChart Series="{Binding SeriesData}" LegendLocation="Left">
                            <lvc:CartesianChart.AxisX>
                                <lvc:Axis Title="Time" Labels="{Binding XLabel}">
                                    <lvc:Axis.Separator>
                                        <lvc:Separator Step="1"/>
                                    </lvc:Axis.Separator>
                                </lvc:Axis>
                            </lvc:CartesianChart.AxisX>
                            <lvc:CartesianChart.AxisY>
                                <lvc:Axis Title="Value" Labels="{Binding Values}"/>
                            </lvc:CartesianChart.AxisY>
                        </lvc:CartesianChart>

                    </Grid>
                    <!-- 이벤트 로그 머리말 Title -->
                    <Grid Grid.Column="1" Grid.Row="0" Background="LightGray" Margin="2,0,2,0">
                        <StackPanel Orientation="Horizontal">
                            <StackPanel.Resources>
                                <Style TargetType="{x:Type Button}">
                                    <Setter Property="Foreground" Value="White"/>
                                    <Setter Property="Width" Value="76"/>
                                    <Setter Property="Margin" Value="3,0,3,0"/>
                                    <Setter Property="HorizontalAlignment" Value="Center"/>
                                    <Setter Property="VerticalAlignment" Value="Center"/>
                                    <Style.Resources>
                                        <Style TargetType="{x:Type Border}">
                                            <Setter Property="CornerRadius" Value="8"/>
                                        </Style>
                                    </Style.Resources>

                                </Style>
                            </StackPanel.Resources>
                            <TextBlock Width="150" Foreground="Black" FontSize="14" Text="EVENT LOG" Margin="20,0,0,0"/>
                            <Button x:Name="FilterAlarm" Content="Alarm" Background="OrangeRed"/>
                            <Button x:Name="FilterWarning" Content="Waring"  Background="Orange"/>
                            <Button x:Name="FilterInformation" Content="Information" Background="LimeGreen"/>
                            <Button x:Name="FilterClear" Content="Clear" Background="DarkGray"/>
                        </StackPanel>
                    </Grid>
                    <ListView x:Name="EventLog" Grid.Column="1" Grid.Row="1">
                        <ListView.View>
                            <GridView>
                                <GridViewColumn Header="Date" Width="65">
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <!-- 이벤트 발생 날짜 -->
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>
                                <GridViewColumn Header="Time" Width="65">
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <!-- 이벤트 발생 시간 -->
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>
                                <GridViewColumn DisplayMemberBinding ="{Binding EventInput}" Header="Event" Width="130"/>
                                <GridViewColumn DisplayMemberBinding ="{Binding EventInput}" Header="Priority" Width="65"/>
                                <GridViewColumn DisplayMemberBinding ="{Binding EventInput}" Header="Message" Width="195"/>
                            </GridView>
                        </ListView.View>
                    </ListView>


                </Grid>

            </Grid>
            <!-- Start: GridColumn Right -->
            <Grid Grid.Column="2" x:Name="gridMenuRight">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="240"/>
                </Grid.RowDefinitions>
                <Border Grid.Row="0" Background="{DynamicResource SideBackgroundColor}">
                </Border>
                <Grid Grid.Row="0">
                    <StackPanel>
                        <!-- Current State List -->
                        <StackPanel Height="200" VerticalAlignment="Top">
                            <TextBlock Foreground="DarkBlue" FontSize="14" HorizontalAlignment="Center" Margin="2">
                            <Bold>Current State</Bold></TextBlock>
                            <Line X1="5" Y1="0" X2="295" Y2="0" Stroke="DarkBlue" StrokeThickness="2"/>
                            <StackPanel Orientation="Horizontal">
                                <Button x:Name="GasState" FontSize="14" Content="Idle State" Width="100" Margin="10,1,0,1"/>
                                <TextBox x:Name="GasCurrentValue" FontSize="14" Width="100" Margin="10,1,0,1">
                                    100
                                </TextBox>
                                <Ellipse x:Name="GasCurrentState" Width="10" Height="10"
                                     Fill="LawnGreen"
                                     Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <Button x:Name="ChamberState" FontSize="14" Content="Chamber Open" Width="100" Margin="10,1,0,1"/>
                                <TextBox x:Name="ChamberCurrentValue" FontSize="14" Width="100" Margin="10,1,0,1">
                                    100
                                </TextBox>
                                <Ellipse x:Name="ChamberCurrentState" Width="10" Height="10"
                                         Fill="LawnGreen"
                                         Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <Button x:Name="LeakState" FontSize="14" Content="Leak Test" Width="100" Margin="10,1,0,1"/>
                                <TextBox x:Name="LeakCurrentValue" FontSize="14" Width="100" Margin="10,1,0,1">
                                    Leak Test Mode
                                </TextBox>
                                <Ellipse x:Name="LeakCurrentState" Width="10" Height="10"
                                         Fill="LawnGreen"
                                         Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <Button x:Name="SoftStopState" FontSize="14" Content="Soft STOP" Width="100" Margin="10,1,0,1"/>
                                <TextBox x:Name="SoftStopValue" FontSize="14" Width="100" Margin="10,1,0,1">
                                    50
                                </TextBox>
                                <Ellipse x:Name="SoftStopCurrentState" Width="10" Height="10"
                                         Fill="LawnGreen"
                                         Margin="10,0,0,0"/>
                            </StackPanel>
                        </StackPanel>
                        <!-- Enable State List -->
                        <StackPanel Height="230" VerticalAlignment="Top">
                            <TextBlock Foreground="DarkBlue" FontSize="14" HorizontalAlignment="Center" Margin="2">
                        <Bold>Enable State</Bold></TextBlock>
                            <Line X1="5" Y1="0" X2="295" Y2="0" Stroke="DarkBlue" StrokeThickness="2"/>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Recipe Allowed</TextBlock>
                                <Ellipse x:Name="RecipeAllowedState" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Control Allowed</TextBlock>
                                <Ellipse x:Name="ControlAllowedState" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">H2 Gas Supply</TextBlock>
                                <Ellipse x:Name="H2GasSupplyState" Width="10" Height="10"
                                 Fill="Red"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Hydride Gas Supply</TextBlock>
                                <Ellipse x:Name="HydrideGasSupplyState" Width="10" Height="10"
                                 Fill="Red"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Reactor Open Enable</TextBlock>
                                <Ellipse x:Name="ReactorOpenState" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Reactor Open Timer</TextBlock>
                                <Ellipse x:Name="ReactorOpenTime" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Heating Allowed</TextBlock>
                                <Ellipse x:Name="HeatingAllowedState" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Double O-Ring</TextBlock>
                                <Ellipse x:Name="DORState" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                        </StackPanel>
                        <!-- Utility State -->
                        <StackPanel Height="200" VerticalAlignment="Top">
                            <TextBlock Foreground="DarkBlue" FontSize="14" HorizontalAlignment="Center" Margin="2">
                        <Bold>Utility State</Bold></TextBlock>
                            <Line X1="5" Y1="0" X2="295" Y2="0" Stroke="DarkBlue" StrokeThickness="2"/>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Pneumatic Pressure</TextBlock>
                                <Ellipse x:Name="PneumaticPressureState" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Cooling Water</TextBlock>
                                <Ellipse x:Name="CoolingWaterState" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Reactor Cabinet Door</TextBlock>
                                <Ellipse x:Name="ReactorDoorState" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Gas Cabinet Door</TextBlock>
                                <Ellipse x:Name="GasDoorState" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Control Cabinet Door</TextBlock>
                                <Ellipse x:Name="ControlDoorState" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                        </StackPanel>
                        <!-- Alarm State -->
                        <StackPanel  VerticalAlignment="Top">
                            <TextBlock Foreground="DarkBlue" FontSize="14" HorizontalAlignment="Center" Margin="2">
                        <Bold>Equipment State</Bold></TextBlock>
                            <Line X1="5" Y1="0" X2="295" Y2="0" Stroke="DarkBlue" StrokeThickness="2"/>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Alarm</TextBlock>
                                <Ellipse x:Name="AlarmState" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Warning</TextBlock>
                                <Ellipse x:Name="WarningState" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="200">Silence</TextBlock>
                                <Ellipse x:Name="SilenceState" Width="10" Height="10"
                                 Fill="LawnGreen"
                                 Margin="10,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock FontSize="14" Margin="20,2,2,2" Width="170">User</TextBlock>
                                <TextBox Width="100">Administartor</TextBox>
                            </StackPanel>
                        </StackPanel>
                    </StackPanel>
                </Grid>
                <Border Grid.Row="1" Background="{DynamicResource BottomBackgroundColor}"
                        BorderBrush="Black" BorderThickness="1">
                </Border>
                <!-- Device Information Block -->
                <Grid Grid.Row="1" >
                    <Grid.Resources>
                        <Style x:Key="Analog Device Stroke" TargetType="Rectangle">
                            <Setter Property="Stroke" Value="Black"/>
                            <Setter Property="StrokeThickness" Value="1"/>
                        </Style>
                    </Grid.Resources>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="120"/>
                        <ColumnDefinition/>

                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="25"/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Rectangle Grid.ColumnSpan="3" Style="{StaticResource Analog Device Stroke}" Fill="LightGray"/>
                    <TextBlock Grid.ColumnSpan="3" Grid.Row="0" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center">
                        Analog Device Information
                    </TextBlock>
                    <!-- Type of Value -->
                    <Rectangle Grid.Column="0" Grid.Row="1" Style="{StaticResource Analog Device Stroke}"/>
                    <TextBlock Grid.Column="0" Grid.Row="1" FontSize="12" VerticalAlignment="Center" Margin="5,0,0,0">
                        Type of Value
                    </TextBlock>
                    <Rectangle Grid.Column="0" Grid.Row="2" Style="{StaticResource Analog Device Stroke}" Fill="Lavender"/>
                    <TextBlock Grid.Column="0" Grid.Row="2" FontSize="12" VerticalAlignment="Center" Margin="5,0,0,0" >
                        Current Value
                    </TextBlock>
                    <Rectangle Grid.Column="0" Grid.Row="3" Style="{StaticResource Analog Device Stroke}"/>
                    <TextBlock Grid.Column="0" Grid.Row="3" FontSize="12" VerticalAlignment="Center" Margin="5,0,0,0">
                        Setting Value
                    </TextBlock>
                    <Rectangle Grid.Column="0" Grid.Row="4" Style="{StaticResource Analog Device Stroke}" Fill="Lavender"/>
                    <TextBlock Grid.Column="0" Grid.Row="4" FontSize="12" VerticalAlignment="Center" Margin="5,0,0,0" >
                        Max. Value
                    </TextBlock>
                    <Rectangle Grid.Column="0" Grid.Row="5" Style="{StaticResource Analog Device Stroke}"/>
                    <TextBlock Grid.Column="0" Grid.Row="5" FontSize="12" VerticalAlignment="Center" Margin="5,0,0,0">
                        Alarm Deviation
                    </TextBlock>
                    <Rectangle Grid.Column="0" Grid.Row="6" Style="{StaticResource Analog Device Stroke}" Fill="Lavender"/>
                    <TextBlock Grid.Column="0" Grid.Row="6" FontSize="12" VerticalAlignment="Center" Margin="5,0,0,0">
                        Warning Deviation
                    </TextBlock>
                    <Rectangle Grid.Column="0" Grid.Row="7" Style="{StaticResource Analog Device Stroke}"/>
                    <TextBlock Grid.Column="0" Grid.Row="7" FontSize="12" VerticalAlignment="Center" Margin="5,0,0,0">
                        Ramping Time
                    </TextBlock>
                    <Rectangle Grid.Column="0" Grid.Row="8" Style="{StaticResource Analog Device Stroke}" Fill="Lavender"/>
                    <TextBlock Grid.Column="0" Grid.Row="8" FontSize="12" VerticalAlignment="Center" Margin="5,0,0,0">
                        Note
                    </TextBlock>
                    <!-- Analog Value -->
                    <Rectangle Grid.Column="1" Grid.Row="1" Style="{StaticResource Analog Device Stroke}"/>
                    <TextBlock Grid.Column="1" Grid.Row="1" FontSize="12" VerticalAlignment="Center" Margin="5,0,0,0">

                    </TextBlock>
                    <Rectangle Grid.Column="1" Grid.Row="2" Style="{StaticResource Analog Device Stroke}" Fill="Lavender"/>
                    <TextBlock Grid.Column="1" Grid.Row="2" FontSize="12" VerticalAlignment="Center" Margin="5,0,0,0" >

                    </TextBlock>
                    <Rectangle Grid.Column="1" Grid.Row="3" Style="{StaticResource Analog Device Stroke}"/>
                    <TextBlock Grid.Column="1" Grid.Row="3" FontSize="12" VerticalAlignment="Center" Margin="5,0,0,0">

                    </TextBlock>
                    <Rectangle Grid.Column="1" Grid.Row="4" Style="{StaticResource Analog Device Stroke}" Fill="Lavender"/>
                    <TextBlock Grid.Column="1" Grid.Row="4" FontSize="12" VerticalAlignment="Center" Margin="5,0,0,0" >

                    </TextBlock>
                    <Rectangle Grid.Column="1" Grid.Row="5" Style="{StaticResource Analog Device Stroke}"/>
                    <TextBlock Grid.Column="1" Grid.Row="5" FontSize="12" VerticalAlignment="Center" Margin="5,0,0,0">

                    </TextBlock>
                    <Rectangle Grid.Column="1" Grid.Row="6" Style="{StaticResource Analog Device Stroke}" Fill="Lavender"/>
                    <TextBlock Grid.Column="1" Grid.Row="6" FontSize="12" VerticalAlignment="Center" Margin="5,0,0,0">

                    </TextBlock>
                    <Rectangle Grid.Column="1" Grid.Row="7" Style="{StaticResource Analog Device Stroke}"/>
                    <TextBlock Grid.Column="1" Grid.Row="7" FontSize="12" VerticalAlignment="Center" Margin="5,0,0,0">

                    </TextBlock>
                    <Rectangle Grid.Column="1" Grid.Row="8" Style="{StaticResource Analog Device Stroke}" Fill="Lavender"/>
                    <TextBlock Grid.Column="1" Grid.Row="8" FontSize="12" VerticalAlignment="Center" Margin="5,0,0,0">

                    </TextBlock>

                </Grid>

            </Grid>
        </Grid>
    </Border>
</Window>
