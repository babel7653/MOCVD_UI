            <Grid DataContext="{Binding DashBoardViewModel}" Grid.Row="1" Background="#3e4a4b">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="20"/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <StackPanel Grid.Column="0" Grid.Row="0" Orientation="Horizontal">
                    <Button Content="LivePlot"/>
                </StackPanel>
                <oxy:PlotView Grid.Row="1" Grid.ColumnSpan="2" x:Name="FlowControlLive" Model="{Binding FlowControlLivePlot}">
                    <oxy:PlotView.ContextMenu>
                        <ContextMenu >
                            <MenuItem Header="{Binding ControlValueOption}" Command="{Binding ShowControlValue}"/>
                            <MenuItem Header="{Binding TargetValueOption}" Command="{Binding ShowTargetValue}"/>
                        </ContextMenu>
                    </oxy:PlotView.ContextMenu>
                </oxy:PlotView>
            </Grid>
        </Grid>
        <Grid Grid.Column="1" Background="{StaticResource DashboardBackground}">
            <Grid.Resources>
                <Style TargetType="{x:Type Label}">
                    <Setter Property="Foreground" Value="{StaticResource PrimaryFontColor}"/>
                    <Setter Property="FontSize" Value="11"/>
                    <Setter Property="Height" Value="30"/>
                    <Setter Property="VerticalAlignment" Value="Bottom"/>
                </Style>
                <Style TargetType="{x:Type Button}">
                    <Setter Property="Width" Value="70"/>
                    <Setter Property="Height" Value="20"/>
                    <Setter Property="Margin" Value="4"/>
                    <Setter Property="FontSize" Value="11"/>
                </Style>
                <Style TargetType="{x:Type TextBlock}">
                    <Setter Property="Foreground" Value="{StaticResource PrimaryFontColor}"/>
                    <Setter Property="FontSize" Value="11"/>
                    <Setter Property="HorizontalAlignment" Value="Center"/>
                </Style>
                <Style TargetType="{x:Type TextBox}">
                    <Setter Property="Width" Value="64"/>
                    <Setter Property="Height" Value="22"/>
                    <Setter Property="Background" Value="LightGray"/>
                    <Setter Property="Foreground" Value="DarkBlue"/>
                    <Setter Property="FontSize" Value="11"/>
                    <Setter Property="HorizontalAlignment" Value="Center"/>
                    <Setter Property="VerticalAlignment" Value="Center"/>
                    <Setter Property="TextAlignment" Value="Right"/>
                </Style>
            </Grid.Resources>
            <Grid.RowDefinitions>
                <RowDefinition Height="100"/>
                <RowDefinition Height="180"/>
                <RowDefinition Height="240"/>
                <RowDefinition Height="180"/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <!-- User Status-->
            <Grid Grid.Row="0">
                <StackPanel Orientation="Vertical">
                    <Label Foreground="DeepSkyBlue" FontSize="14" HorizontalContentAlignment="Center">
                        <Bold>User Status </Bold>
                    </Label>
                    <Line X1="5" Y1="0" X2="295" Y2="0" Stroke="DeepSkyBlue" StrokeThickness="2" Margin="2,2,2,8"/>
                    <Button Content="User State" Width="240" Height="24" Margin="4" Command="{Binding ManualBatchCommand}"/>
                </StackPanel>
            </Grid>
            <!-- Susceptor Temperature Control Current Status -->
            <Grid Grid.Row="1">
                <StackPanel Orientation="Vertical">
                    <Label Foreground="DeepSkyBlue" FontSize="14" HorizontalContentAlignment="Center">
                        <Bold>Heater Controller</Bold>
                    </Label>
                    <Line X1="5" Y1="0" X2="295" Y2="0" Stroke="DeepSkyBlue" StrokeThickness="2"/>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="22"/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <TextBlock Grid.Column="0" Grid.Row="0" Text="Target Val." Margin="0,6,0,0"/>
                        <TextBlock Grid.Column="1" Grid.Row="0" Text="Control Val." Margin="0,6,0,0"/>
                        <TextBlock Grid.Column="2" Grid.Row="0" Text="Current Val." Margin="0,6,0,0" />
                        <TextBlock Grid.Column="3" Grid.Row="0" Text="Power Rate" Margin="0,6,0,0"/>
                        <TextBox x:Name="tbTargetTemp"  Grid.Column="0" Grid.Row="1"  Text="{Binding TargetTemp}"/>
                        <TextBox x:Name="tbControlTemp"  Grid.Column="1" Grid.Row="1" Text="{Binding ControlTemp}"/>
                        <TextBox x:Name="tbCurrentTemp"  Grid.Column="2" Grid.Row="1" Text="{Binding CurrentTemp}"/>
                        <TextBox x:Name="tbPowerRateTemp" Grid.Column="3" Grid.Row="1" Text="{Binding PowerRateTemp}"/>
                    </Grid>
                    <StackPanel Orientation="Horizontal" Height="30">
                        <TextBlock Width="210"  Height="20" TextAlignment="Right" Text="Heater Control Mode"
                                Margin="0,2,6,0"/>
                        <Button Height="20" Width="80" FontSize="11" Content="{Binding InputManualAuto}" />
                    </StackPanel>
                    <Label Content="Induction Heater Control" VerticalContentAlignment="Bottom"/>
                    <StackPanel Orientation="Horizontal" Height="30" Margin="10,0,0,0">
                        <Button x:Name="btHeaterOn" Content="{Binding InductionHeaterOn}"/>
                        <Button x:Name="btHeaterReset" Content="{Binding InductionHeaterReset}"/>
                    </StackPanel>
                </StackPanel>
            </Grid>
            <!-- Reactor Pressure Control Current Status -->
            <Grid Grid.Row="2">
                <StackPanel Orientation="Vertical">
                    <Label Foreground="DeepSkyBlue" FontSize="14" HorizontalContentAlignment="Center">
                        <Bold>Pressure Controller</Bold>
                    </Label>
                    <Line X1="5" Y1="0" X2="295" Y2="0" Stroke="DeepSkyBlue" StrokeThickness="2"/>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="22"/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <TextBlock Grid.Column="0" Grid.Row="0" Text="Target Val." Margin="0,6,0,0"/>
                        <TextBlock Grid.Column="1" Grid.Row="0" Text="Control Val." Margin="0,6,0,0"/>
                        <TextBlock Grid.Column="2" Grid.Row="0" Text="Current Val." Margin="0,6,0,0"/>
                        <TextBlock Grid.Column="3" Grid.Row="0" Text="Position" Margin="0,6,0,0"/>
                        <TextBox x:Name="tbTargetPress"  Grid.Column="0" Grid.Row="1"  Text="{Binding TargetPress}"/>
                        <TextBox x:Name="tbControlPress"  Grid.Column="1" Grid.Row="1" Text="{Binding ControlPress}"/>
                        <TextBox x:Name="tbCurrentPress"  Grid.Column="2" Grid.Row="1" Text="{Binding CurrentPress}"/>
                        <TextBox x:Name="tbPosition" Grid.Column="3" Grid.Row="1" Text="{Binding ValvePosition}"/>
                    </Grid>
                    <StackPanel Orientation="Horizontal" Height="30">
                        <Label Width="150" VerticalContentAlignment="Bottom" Content="Pressure Control Mode" />
                        <Button Height="20" FontSize="11" Content="{Binding PressureControlMode}" />
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Height="30">
                        <Label  Width="150" VerticalContentAlignment="Bottom" Content="Throttle Valve Mode" />
                        <Button x:Name="btThrottleRun" Content="{Binding ThrottleValveControlMode}"/>
                    </StackPanel>

                    <StackPanel Orientation="Horizontal" Height="30">
                        <Label Width="150" VerticalContentAlignment="Bottom" Content="Throttle Valve Status" />
                        <Button x:Name="btTrottleStatus" Content=""/>
                    </StackPanel>
                    <Label Content="Vacuum Pump Control" VerticalContentAlignment="Bottom"/>
                    <StackPanel Orientation="Horizontal" Height="30" Margin="10,0,0,0">
                        <Button x:Name="btPumpOn" Content="{Binding VaccumPumpOn}"/>
                        <Button x:Name="btPumpReset" Content="{Binding VaccumPumpReset}"/>
                    </StackPanel>
                </StackPanel>
            </Grid>
            <!-- Susceptor Rotation Current Status -->
            <Grid Grid.Row="3">
                <StackPanel Orientation="Vertical">
                    <Label Foreground="DeepSkyBlue" FontSize="14" HorizontalContentAlignment="Center">
                        <Bold>Rotation Controller</Bold>
                    </Label>
                    <Line X1="5" Y1="0" X2="295" Y2="0" Stroke="DeepSkyBlue" StrokeThickness="2"/>
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="22"/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <TextBlock Grid.Column="0" Grid.Row="0" Text="Target Val." Margin="0,6,0,0"/>
                        <TextBlock Grid.Column="1" Grid.Row="0" Text="Control Val." Margin="0,6,0,0"/>
                        <TextBlock Grid.Column="2" Grid.Row="0" Text="Current Val." Margin="0,6,0,0"/>
                        <TextBox x:Name="tbTargetRotation"  Grid.Column="0" Grid.Row="1"  Text="{Binding TargetRotation}"/>
                        <TextBox x:Name="tbControlRotation"  Grid.Column="1" Grid.Row="1" Text="{Binding ControlRotation}"/>
                        <TextBox x:Name="tbCurrentRotation"  Grid.Column="2" Grid.Row="1" Text="{Binding CurrentRotation}"/>
                    </Grid>
                        <Label Content="Motor Control" VerticalContentAlignment="Bottom"/>
                        <Button x:Name="btMotorReset" Content="{Binding RotationReset}" HorizontalAlignment="Left" Margin="10,0,0,0"/>
                </StackPanel>
            </Grid>
            <!-- Event Log -->
            <Grid Grid.Row="4" >
                <StackPanel Orientation="Vertical">
                    <StackPanel Orientation="Horizontal">
                        <Label Foreground="DeepSkyBlue" FontSize="14" HorizontalContentAlignment="Center" Margin="100,0,0,0">
                            <Bold>Event Log</Bold>
                        </Label>
                        <Button Height="20" Width="80" Content="Clear"/>
                    </StackPanel>
                    <Line X1="5" Y1="0" X2="295" Y2="0" Stroke="DeepSkyBlue" StrokeThickness="2" Margin="2,2,2,8"/>
                    <DataGrid x:Name="dgEventLog">
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="Type" Binding="{Binding Type}" Width="70"/>
                            <DataGridTextColumn Header="Message" Binding="{Binding Message}" Width="160"/>
                            <DataGridTextColumn Header="Date/Time" Binding="{Binding Date}" Width="70"/>
                        </DataGrid.Columns>
                    </DataGrid>
                </StackPanel>
            </Grid>
        </Grid>
    </Grid>
</Page>
