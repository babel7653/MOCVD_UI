			<Grid Grid.Row="2" Background="AliceBlue" Grid.ColumnSpan="4">
				<Grid.Resources>
					<Style TargetType="{x:Type DataGrid}">
						<Setter Property="HorizontalGridLinesBrush" Value="Blue"/>
						<Setter Property="VerticalGridLinesBrush" Value="Blue"  />
					</Style>
					<Style TargetType="{x:Type DataGridRow}">
						<Style.Triggers>
							<Trigger Property="IsSelected" Value="True">
								<Setter Property="FontSize" Value="15" />
								<Setter Property="FontWeight" Value="Bold" />
							</Trigger>
						</Style.Triggers>
						<Setter Property="Height" Value="25"/>
					</Style>
					<Style TargetType="{x:Type DataGridColumnHeader}">
						<Setter Property="Foreground" Value="Black"/>
						<Setter Property="FontSize" Value="12"/>
						<Setter Property="BorderBrush" Value="blue" />
						<Setter Property="BorderThickness" Value="1" />
						<Setter Property="HorizontalContentAlignment" Value="Center"/>
						<Setter Property="Padding" Value="5,0"/>
						<Setter Property="SeparatorVisibility" Value="Visible"/>
					</Style>
					<Style x:Key="Content1" TargetType="{x:Type TextBlock}">
						<Setter Property="Foreground" Value="Black"/>
						<Setter Property="FontSize" Value="12"/>
						<Setter Property="VerticalAlignment" Value="Center"/>
						<Setter Property="HorizontalAlignment" Value="Center"/>
					</Style>
					<Style x:Key="Content2" TargetType="{x:Type TextBlock}">
						<Setter Property="Foreground" Value="Black"/>
						<Setter Property="FontSize" Value="12"/>
						<Setter Property="VerticalAlignment" Value="Center"/>
					</Style>
				</Grid.Resources>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="68*"/>
					<ColumnDefinition Width="97*"/>
				</Grid.ColumnDefinitions>
				<!-- 수직형 탭 설정 -->
				<TabControl TabStripPlacement="Left" Grid.ColumnSpan="2" >
					<TabControl.Resources>
						<Style TargetType="{x:Type TabItem}">
							<Setter Property="HeaderTemplate">
								<Setter.Value>
									<DataTemplate>
										<ContentPresenter Content="{TemplateBinding Content}">
											<ContentPresenter.LayoutTransform>
												<RotateTransform Angle="270"/>
											</ContentPresenter.LayoutTransform>
										</ContentPresenter>
									</DataTemplate>
								</Setter.Value>
							</Setter>
							<Setter Property="Width" Value="20"/>
							<Setter Property="Height" Value="90"/>
							<Setter Property="Padding" Value="3"/>
						</Style>
					</TabControl.Resources>
					<!-- 수직형 탭 Reactor Recipe Data  -->
					<TabItem x:Name="ReactorData" Header="Reactor Data">
						<TabItem.Content>
							<DataGrid x:Name="RecipeStepReactor" AutoGenerateColumns="False" Padding="5" 
																	SelectedIndex="{Binding CurrRcpNum, Mode=TwoWay}"
																	CanUserSortColumns="False"
																	ScrollViewer.ScrollChanged="ScrollChanged"
																	CanUserAddRows="False" IsReadOnly="True"
																	ItemsSource="{Binding Recipes}">
								<DataGrid.Columns>
									<DataGridTextColumn Header="No." Binding="{Binding No}" ElementStyle="{StaticResource Content1}" Width="30"/>
									<DataGridTextColumn Header="Name" Binding="{Binding Name}" ElementStyle="{StaticResource Content2}" Width="150"/>
									<DataGridTextColumn Header="Ramp" Binding="{Binding rTime}" ElementStyle="{StaticResource Content1}" Width="50"/>
									<DataGridTextColumn Header="Hold" Binding="{Binding hTime}" ElementStyle="{StaticResource Content1}" Width="50"/>
									<DataGridTextColumn Header="Susceptor Temp(℃)" Binding="{Binding sTemp}" ElementStyle="{StaticResource Content1}" Width="120"/>
									<DataGridTextColumn Header="Reactor Pressure(torr)" Binding="{Binding rPress}" ElementStyle="{StaticResource Content1}" Width="140"/>
									<DataGridTextColumn Header="Susceptor Rotate(rpm)" Binding="{Binding sRotation}" ElementStyle="{StaticResource Content1}" Width="140"/>
									<DataGridTextColumn Header="Compare Temp(℃)" Binding="{Binding cTemp}" ElementStyle="{StaticResource Content1}" Width="120"/>
									<DataGridTextColumn Header="Loop" Binding="{Binding Loop}" ElementStyle="{StaticResource Content1}" Width="50"/>
									<DataGridTextColumn Header="Jump" Binding="{Binding Jump}" ElementStyle="{StaticResource Content1}" Width="50"/>
								</DataGrid.Columns>
							</DataGrid>
